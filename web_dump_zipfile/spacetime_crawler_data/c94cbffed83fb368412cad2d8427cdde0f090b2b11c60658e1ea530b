<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
<script>
var isMobile = false;

$(document).ready(function() {
    
    if(/Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ) {
        isMobile = true;

        var tooltip1 = document.getElementById("tooltip1");
        var tooltip2 = document.getElementById("tooltip2");
        var tooltip3 = document.getElementById("tooltip3");
        var num1 = document.getElementById("num1");
        var num2 = document.getElementById("num2");
        var num3 = document.getElementById("num3");
        tooltip1.removeAttribute("href");
        tooltip2.removeAttribute("href");
        tooltip3.removeAttribute("href");
        num1.removeAttribute("onmouseover");
        num1.removeAttribute("onmouseout");
        num2.removeAttribute("onmouseover");
        num2.removeAttribute("onmouseout");
        num3.removeAttribute("onmouseover");
        num3.removeAttribute("onmouseout");
        tooltip1.setAttribute("onclick","showDesc(1)");
        tooltip2.setAttribute("onclick","showDesc(2)");
        tooltip3.setAttribute("onclick","showDesc(3)");

        var divsToHide = document.getElementsByClassName("close-tool");
        for(var i = 0; i < divsToHide.length; i++){
            divsToHide[i].style.display = "none"; 
        }
    
    }else{

    }    
});

function showDesc(number){
    var item = "show"+number;
    document.getElementById(item).style.display = "block";

    if(isMobile){
        document.getElementById("tooltip"+number).removeAttribute("onclick");
        document.getElementById("close"+number).style.display = "block";
    }

};
function hideDesc(number){
    var item = "show"+number;
    document.getElementById(item).style.display = "none";

    if(isMobile){
        document.getElementById("close"+number).style.display = "none";
        document.getElementById("tooltip"+number).setAttribute("onclick","showDesc("+number+")");
    }
};
</script>
