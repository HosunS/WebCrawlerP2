                (new Image()).src = '/account/js-reporting/?rid=344h1lhe513p4&crumb=' + encodeURIComponent('zsYH89NtqwF') + '&message=' + encodeURIComponent(name.toLowerCase()) + '&url=' + encodeURIComponent(url);
