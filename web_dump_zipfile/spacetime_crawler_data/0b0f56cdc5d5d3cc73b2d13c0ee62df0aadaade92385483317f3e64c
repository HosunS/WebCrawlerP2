<div class="custom"  >
	<script type="text/javascript">
//upcoming events filter
jQuery(document).ready(function() {
	//jQuery('.filter-cat select').val('Sort By'); //set default
	jQuery('.filter-cat .drop-arrow').click(function(){
		jQuery('.filter-cat select').trigger('click');
	});
	jQuery('.filter-cat select').change(function() {
		var cat = jQuery(this).val();
		if (cat) {
			jQuery.ajax({
			  url: '/images/files/includes/filter-category.php?cat_id='+cat,
			  context: document.body
			}).done(function(data) {
				jQuery('ul.jcalpro_events').html(data);
			});
		}
	});
