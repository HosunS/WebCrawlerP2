    StyleHatch.indexPage = true;
    

    StyleHatch.navBar = true;

    StyleHatch.infiniteScroll = true;
    StyleHatch.loadingText = "Loading";
    StyleHatch.loadMoreText = "Load more posts";
    StyleHatch.noMoreText = "No more posts";

    StyleHatch.isotope = false;
    if(top===self) {
        StyleHatch.customizeMode=false;
    } else if (location.hostname.toString()=="assets.txmblr.com") {
        StyleHatch.customizeMode=true;
    } else {
        StyleHatch.customizeMode=false;
    }
