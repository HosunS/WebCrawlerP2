		<script type="text/javascript">
			var imgcount = 0;
			var carouselElement = $(".carousel");
			var numSlides = $('.carousel-indicators li').length;

			$(document).ready(function() {
			    checkWindowView();
			});

			$(window).resize(function() {
			    checkWindowView();
			});

			function checkWindowView() {
			    if ($(".navbar").css("display") == "none") {
			        runCarouselMobile();
			    } else {
			        runCarousel();
			    }
			}

			function runCarousel() {

			    carouselElement.carousel({
			        interval: 10000,
			        pause: "hover",
			    });

			    carouselElement.on('slid.bs.carousel', function() {
			        imgcount++;
			        if (imgcount >= numSlides) {
			            $(this).carousel("pause");
			            $(this).carousel({
			                pause: true,
			                interval: false
			            });
			        }
			    });
			}

			function runCarouselMobile() {
			    carouselElement.carousel("pause");
			    carouselElement.carousel({
			        pause: true,
			        interval: false
			    });
			}
		</script><script type="text/javascript">
