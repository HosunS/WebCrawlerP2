<script type="text/javascript" charset="utf-8">
	var wanted_id= 'none', retrieved_id= '0', displayed_id= '0', cursor_in= "0", retrieved_data= "", xxval, yyval;
	var highlightedACV= 0;
  ol_texts= new Array();
  ol_caps= new Array();
  function showsumm(fbid) {
	 cursor_in= fbid;
	 wanted_id= fbid;
	 if( retrieved_id != fbid ) {
		var kw= {
			url: "/cgi-bin/get_summary.pl?"+fbid,
			load: function(type,data,evt){ 
				if( data!="" ) { 
					retrieved_id= fbid; retrieved_data= data;
					displaysumm(fbid); 
					return true; } 
				},
			mimetype: "text/html"
			};
		dojo.io.bind(kw);
		}
	 else {
		displaysumm(fbid);
		}
	 return true;
	}
  function showdata(fbid) {
	 cursor_in= fbid;
	 wanted_id= fbid;
	 if( retrieved_id != fbid ) {
		var kw= {
			url: "/cgi-bin/get_interactions.pl?"+fbid,
			load: function(type,data,evt){ 
				if( data!="" ) { 
					retrieved_id= fbid; retrieved_data= data;
					displaysumm(fbid); 
					return true; } 
				},
			mimetype: "text/html"
			};
		dojo.io.bind(kw);
		}
	 else {
		displaysumm(fbid);
		}
	 return true;
	}
  function displaysumm(fbid) {
	 if( wanted_id !=fbid ) return false;
	 if( cursor_in !=fbid ) return false;
		ol_bgcolor= '#000000';
		ol_fgcolor= '#ffffee';
		overlib(retrieved_data,FULLHTML,WRAP,NOCLOSE,OFFSETX,2,OFFSETY,10,VAUTO,HAUTO,DELAY,500); 
	 return true;
	}
  function delsumm() {
		cursor_in= "0";
		nd();
	 return true;
	}
 function getRealLeft(el) {
	xPos = el.offsetLeft;
	tempEl = el.offsetParent;
	while (tempEl != null) {
		xPos += tempEl.offsetLeft;
		tempEl = tempEl.offsetParent;
		}
	return xPos;
 }

 function getRealTop(el) {
	yPos = el.offsetTop;
	tempEl = el.offsetParent;
	while (tempEl != null) {
		yPos += tempEl.offsetTop;
		tempEl = tempEl.offsetParent;
		}
	return yPos;
 }
	function getCSSElementByID(elem) {
         var theElement;
         if (document.getElementById) { theElement = document.getElementById(elem); }
         else { if (document.all) { theElement = document.elem; } }
         return(theElement);
   }
	function showhide(a) { return; }
 function  checkEvt(evt,thisForm,ffield) {
	if( displayed_id != '0' ) {
		evt= (evt) ? evt : event;
		var charCode= (evt.charCode) ? evt.charCode : ((evt.which) ? evt.which : evt.keyCode);
		if( charCode==13 ) showDict(evt,thisForm,ffield);
		}
	return true;
	}
 function showDict(evt,thisForm,ffield) {
	if( displayed_id != '0' ) {
		acvobj= document.getElementById('nACvalues');
		var nmax= acvobj.value;
		evt= (evt) ? evt : event;
		var charCode= (evt.charCode) ? evt.charCode : ((evt.which) ? evt.which : evt.keyCode);
		switch( charCode ) {
			case 40: // arrow down
				if( highlightedACV<nmax ) { 
					highlightedACV= highlightedACV+1;  
					}
				else { highlightedACV= 1; }
				highlightACV(highlightedACV);
				return(true);
			case 38: // arrow up
				if( highlightedACV>1 ) { 
					highlightedACV= highlightedACV-1;  
					}
				else { highlightedACV= nmax; }
				highlightACV(highlightedACV);
				return(true);
			case 13: // enter
				var acvid= 'ACV' + highlightedACV;
				acvobj= document.getElementById(acvid);
				useACValue(acvobj,acvobj.title);
				deldict_overlib();
				return(false);
			}
		}
	deldict_overlib();
	xxval= getRealLeft(ffield);
	yyval= getRealTop(ffield)+10;
	var scripturl= '/cgi-bin/get_dict.pl?format=html&';
	var db= 'fbgn';
	var context= ffield.value;
	if( context.indexOf('[')>0 ) db= 'fbal';
	var fieldn= 'SYMBOL';
	var fbid= 'caller=IB&fld='+db+'-'+fieldn+'&context='+ffield.value+'&species=Dmel';
	if( retrieved_id != fbid ) {
		nd();
		wanted_id= fbid;
		retrieved_id= '0'; 
		retrieved_data= "";
		var kw= {
			url: scripturl+fbid,
			load: function(type,data,evt){ 
				if( data!="" ) { 
					retrieved_id= fbid; retrieved_data= data;
					displaydict_overlib(fbid); 
					return true; } 
				},
			mimetype: "text/html"
			};
		dojo.io.bind(kw);
		}
	else {
		displaydict_overlib(fbid);
		}
	return true;
	}
 function useACValue(acvobj,ttt) {
	//var ttt= acvobj.title;
	var aa= ttt.split('=');
	var target= aa[0];
	if( target=='IB.items' ) {
		document.IB.items.value= unescape(aa[1]);
		//document.editor.submit(); 
		}
	return true;
	}
 function highlightACV(num) {
	var nn= 0;
	var acvid;
	for( nn=0; nn++<20; ) { 
		acvid= 'ACV' + nn;
		acvobj= document.getElementById(acvid);
		if( acvobj ) {
			if( nn==num ) {
				acvobj.style.background= '#cccccc';
				acvobj.style.fontWeight= 'bold';
				highlightedACV= num;
				}
			else {
				acvobj.style.background= '#f8f8f8';
				acvobj.style.fontWeight= 'normal';
				}
			}
		else { nn=100; }
		}
	return true;
	}
 function displaydict_overlib(fbid) {
	if( wanted_id !=fbid ) return true;
		ol_bgcolor= '#000000';
		ol_fgcolor= '#ffffee';
		//overlib(retrieved_data,FULLHTML,WRAP,STICKY,NOCLOSE,TIMEOUT,4000,FIXX,xxval,FIXY,yyval); 
		overlib(retrieved_data,FULLHTML,WRAP,STICKY,NOCLOSE,FIXX,xxval,FIXY,yyval); 
		displayed_id= fbid;
	return true;
	}
 function deldict_overlib() {
	if( displayed_id != '0' ) {
		displayed_id= '0';
		highlightedACV= 0;
		nd();
		}
	return true;
	}
 </script>
