			<script>
					var configProfile = {
						"profile": {"screenName": 'LUCI_at_UCI'},
						"domId": 'twitter-feed',
						"maxTweets": 4,
						"enableLinks": true, 
						"showUser": false,
						"showTime": false,
						"showImages": true,
						"showInteraction": false,
						"customCallback": handleTweets,
						"lang": 'en'
					};
					twitterFetcher.fetch(configProfile);
					function handleTweets(tweets){
							var x = tweets.length;
							var n = 0;
						while(n < x) {
							$("#twitter-feed").append( '<li class="twitter-twitt">' + tweets[n] + '</li>' );
								n++;
							}
					}
			</script>
