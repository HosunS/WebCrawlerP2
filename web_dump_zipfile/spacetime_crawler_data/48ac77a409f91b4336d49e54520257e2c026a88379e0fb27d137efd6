% jesterOut( PredictionMatrix [, filename])
%   extract the test entries from a matrix of predicted values and write them to a file
%   filename = optional filename (default = prediction.txt)
%
% Note: requires the file "jester-test.csv" to be present in the current directory

  if (nargin < 2) filename = 'prediction.txt'; end;
  tmp=load('jester-test.csv');
  users=tmp(:,1); items=tmp(:,2);
  idx = sub2ind(size(Pred),users,items);
  vals = Pred(idx);
  fhandle = fopen(filename,'w');
  fprintf(fhandle, '%2.3f\n',vals);
  fclose(fhandle);

