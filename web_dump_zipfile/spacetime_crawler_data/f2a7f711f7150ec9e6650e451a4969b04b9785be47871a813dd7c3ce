          prevText:       '<i class="fa fa-chevron-left"></i>',             // Sets custom next button text
          autoControls:     false,                              // Builds start/stop controls
          autoControlsCombine:  true,                             // When slideshow is playing only "Stop" control is displayed and vice-versa
          pager:          (jQuery(".bx_slider>.bx_slide").length > 1) ? true: false   // Builds navigation links/buttons if more than one slide
      });

      jQuery('#ContentUCIFund').appendTo('#ContentSideNav');

      // takes image in header and places it into the background. Does not do this when logged in as an admin 
      if (jQuery("#CmsMasterMenu").length > 0){
      // if logged in, do nothing
      } else {
      var srcimg = jQuery(".fullImg img").attr("src");
        jQuery(".fullImg").attr('style', 'background-image:url(' + srcimg + ')');
        jQuery(".fullImg img").hide();
      }

      // event forms remove empty divs
      jQuery(".imod_eventButtons").filter( function() {

        var html = jQuery(this).html();

        if(html == '' || html == '&nbsp;')
            return true;

      }).addClass('emptyP');

    
});
