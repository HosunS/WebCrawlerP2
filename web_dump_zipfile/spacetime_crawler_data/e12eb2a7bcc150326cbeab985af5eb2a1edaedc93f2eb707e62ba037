#ifndef _querygroup_h_
#define _querygroup_h_

#include "common/simmetric.h"

#include <string>

class QueryGroup
{
public:
  const SimMetric &sim;  
  const uint k;
  const uint n;

  std::string* strs;
  uint* lens;
  uint* nosGrams;

  template<class InputIterator> 
  QueryGroup(
    InputIterator ss, uint n, const SimMetric &sim, uint k):
    sim(sim), 
    k(k), 
    n(n)
  {
    strs = new std::string[n];
    lens = new uint[n];
    nosGrams = new uint[n];

    for (uint i = 0; i < n; ++i) {
      strs[i] = *ss;
      lens[i] = ss->length();
      nosGrams[i] = sim.gramGen.getNumGrams(*ss);
      ++ss;
    }
  }
};

#endif
