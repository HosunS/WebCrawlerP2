    <script>
        $.validator.addMethod('educonfirm', function (value, element) {
            var valMembType = $("input:radio[name='Data.OrderDetail.MembershipCode']:checked").val();
            var emailInput = $("input[name='Data.Email']").val();
            if (valMembType == "SD" && (emailInput.slice(-4) !== ".edu")) {
                return false;
            }
            else
            {
                return true;
            }
        }, 'Please enter a valid .edu email address');
        $.validator.unobtrusive.adapters.addBool('educonfirm');

        $.validator.addMethod('emailconfirm', function (value, element) {
            if ($("#Data_Email").val() != $("#Data_ConfirmEmail").val()) {
                return false;
            }
            else
            {
                return true;
            }
        }, 'Emails do not match');
        $.validator.unobtrusive.adapters.addBool('emailconfirm');
    </script>

