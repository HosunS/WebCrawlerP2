print("model: y ~ exp(-t) +  t * exp(-t) + exp(-2t) + t * exp(-2t)");
print("design variable: 0<t<3 discretized");

n=20;
## m=4;
t=3*(1:n)/n;
X=cbind(exp(-t), t*exp(-t), exp(-2*t), t*exp(-2*t));

source('cocktail.r');
w=cocktail(X)[[1]];

print("output:");
print("optimal support points t(i)");
print(t[w>0]);
print("optimal weights w(i)");
print(w[w>0]);
