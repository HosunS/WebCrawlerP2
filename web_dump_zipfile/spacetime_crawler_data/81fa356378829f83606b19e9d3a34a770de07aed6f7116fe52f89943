// Netscape makes Label take a lot of room. Attempt to hack around it.

import java.awt.*;
import gui.*;

class ThinLabel extends Label {
	ThinLabel(String s) { super(s); }
	public Dimension minimumSize() {
		Dimension d = super.minimumSize();
    	FontMetrics metric = PatchFontMetrics.patch(getFontMetrics(getFont()));
//    	return new Dimension(d.width, metric.getHeight()+1);
		return new Dimension(d.width, 15);
	}
	public Dimension preferredSize() { return minimumSize(); }
	public void update(Graphics g) { paint(g); }
}
