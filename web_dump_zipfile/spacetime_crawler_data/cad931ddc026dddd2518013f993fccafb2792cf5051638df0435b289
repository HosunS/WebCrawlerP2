  function setExampleParameters(){
    var reactElem = document.getElementById("id_reactants")
    reactElem.value="CC(=C)[O-].CCCC(=O)CC";
    var tempElem = document.getElementById("id_temperature")
    tempElem.value = 298
    var catElem = document.getElementById("id_cation_solvation_potential")
    catElem.value = 1.0
    var aniElem = document.getElementById("id_anion_solvation_potential")
    aniElem.value = 1.0    
    return false;
  }
</script>


