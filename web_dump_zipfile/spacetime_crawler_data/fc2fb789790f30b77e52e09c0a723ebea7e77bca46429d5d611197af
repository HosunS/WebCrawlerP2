<script type="text/javascript">
	(function() {
		var shares = ["facebook", "twitter", "pinterest", "plusone", "linked_in"];

		for(var i = 0; i < shares.length; ++i) {
			var share = shares[i];
			document.getElementById(share).onclick = showShare;
		}

		var closeButtons = document.querySelectorAll(".mclose");
		for(var i = 0; i < closeButtons.length; ++i) {
			var button = closeButtons[i];
			button.onclick = hideShare;
		}

		function showShare(e) {
			document.getElementById("overlay_block").style.display = "block";
			var overlay = document.getElementById("mimio_share_" + e.target.id);
			overlay.style.display = "block";
			overlay.className += " animation";
		}

		function hideShare(e) {
	    document.getElementById("mimio_share_" + e.target.getAttribute("data-share")).style.display = "none";
	    document.getElementById("overlay_block").style.display = "none";
	    return false;
	  }
	})();
</script>



