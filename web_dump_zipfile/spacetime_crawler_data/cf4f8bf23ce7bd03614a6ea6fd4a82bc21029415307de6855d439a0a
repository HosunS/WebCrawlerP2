<script type='text/javascript'>
    var slideCount;
    function initSlideCount(){
        //$("#block_2_1").html('<div class="col-sm-7 col-md-7 expand"><h4 class="text-center">Now loading... Wait.</h4></div>');
        
        $.ajax({
            url: "/default/frontend/getslidecount",
            data: {},
            dataType: "json",
            global: true,
            cache: false,
            ifModified: false,
            processData: true,
            success: function (datos) {
                slideCount = datos;
                initHomeDesign();
            },
            error: function (request, status, error) {
                mensaje = request.responseText != '' ? request.responseText : 'Timed out or interrupted session, you may be having problems in the network' ;
            },
            timeout: 20000,
            type: "POST"
        });
    }
    
    var homeDesign;
    function initHomeDesign(){
        $.ajax({
            url: "/default/frontend/gethomedesign",
            data: {},
            dataType: "json",
            global: true,
            cache: false,
            ifModified: false,
            processData: true,
            success: function (datos) {
                homeDesign = datos;
                initHomeAdd();
            },
            error: function (request, status, error) {
                mensaje = request.responseText != '' ? request.responseText : 'Timed out or interrupted session, you may be having problems in the network' ;
                $('#acceptbox div').html(mensaje);
                $('#acceptbox').css('background-color', '#E8747D');
            },
            timeout: 20000,
            type: "POST"
        });
    }
    
    var homeAdd = new Array();
    function initHomeAdd(){
        $.ajax({
            url: "/default/frontend/gethomedata",
            data: {},
            dataType: "json",
            global: true,
            cache: false,
            ifModified: false,
            processData: true,
            success: function (datos) {

                homeAdd[2] =    '<div class="col-sm-7 col-md-7">'+
                                '  <div id="mainCarousel" class="carousel slide">'+
                                '    <ol class="carousel-indicators">';

                if(slideCount>1) {
                    for (i = 0; i < slideCount; i++) {
                        homeAdd[2] += ' <li data-target="#mainCarousel" data-slide-to="' + i + '" ';
                        if (i == 0)
                            homeAdd[2] += 'class="active"';
                        homeAdd[2] += ' ></li>';
                    }
                }

                homeAdd[2] +=   '    </ol>'+
                                '   <div class="carousel-inner">';

                var i=0;

                for (data in datos) {
                    if(datos[data]['homeType']=='banner'){
                        homeAdd[datos[data]['homeSquare']] =    '<div class="col-sm-12 col-md-12">'+
                                                                '<div class="alert alert-info">'+
                                                                '<img src="'+datos[data]['homeImageUrl']+'" alt="'+strip_tags(datos[data]['homeTitle'])+'"></img>'+
                                                                '</div>'+
                                                                '</div>';
                    }

                    if(datos[data]['homeType']=='text'){
                        if(datos[data]['homeSquare']=='3'){
                            homeAdd[datos[data]['homeSquare']] =    '<h3 class="text-center">'+datos[data]['homeTitle']+'</h3>'+ /*text on carousel box*/
                                                                     datos[data]['homeText'];
                        }

                    }

                    if(datos[data]['homeType']=='text'){
                        if(datos[data]['homeSquare']=='4'){
                            homeAdd[datos[data]['homeSquare']] =    '<div class="stl-content col-sm-7 col-md-7 expander" id="content-1">'+
                                                                    '    <h3 class="text-center">'+datos[data]['homeTitle']+'</h3>'+ /*main text left*/
                                                                        datos[data]['homeText']+
                                                                    '</div>';
                        }
                    }

                    if(datos[data]['homeType']=='text'){
                        if(datos[data]['homeSquare']=='5'){
                            homeAdd[datos[data]['homeSquare']] =    '<div class="stl-content col-sm-4 col-md-4" id="content-2">'+
                                                                    '   <h3 class="text-center">'+datos[data]['homeTitle']+'</h3>'+
                                                                        datos[data]['homeText']+
                                                                    '</div>';
                        }
                    }
                    
                    if(datos[data]['homeType']=='rss'){
                        if(datos[data]['homeSquare']=='5'){
                            homeAdd[datos[data]['homeSquare']] =    '<div class="stl-content col-sm-4 col-md-4" id="content-3">'+
                                                                    '   <h3 class="text-center">'+datos[data]['homeTitle']+'</h3>'+ /*rss text right*/
                                                                    '   <div id="rss-blog"></div>' +
                                                                    '</div>';
                            rssURL = datos[data]['homeUrl'];
                        }
                    }                    

                    if(datos[data]['homeType']=='slide_i'||datos[data]['homeType']=='slide_v'){
                        var item_active = '';
                        if(i==0)
                            item_active = 'active';

                        homeAdd[datos[data]['homeSquare']] += '<div class="item '+item_active+'">';

                        if(datos[data]['homeImageUrl']!='') {
                            homeAdd[datos[data]['homeSquare']] += '<img width="680" height="400" src="'+datos[data]['homeImageUrl']+'" class="img-polaroid img-responsive" alt="'+strip_tags(datos[data]['homeTitle'])+'">';
                        }

                        if(datos[data]['homeUrl']!='') {
                            homeAdd[datos[data]['homeSquare']] += '<iframe id="player'+i+'" width="670" height="400" src="'+datos[data]['homeUrl']+'" frameborder="0" allowfullscreen></iframe>';
                        }

                        if(datos[data]['homeTitle']!='') {
                            homeAdd[datos[data]['homeSquare']] +=   '<div class="carousel-caption">'+
                                                                    '<p>'+datos[data]['homeTitle']+'</p>'+
                                                                    '</div>';
                        }

                        homeAdd[datos[data]['homeSquare']] += '</div>';

                        i++;
                    }

                }

                homeAdd[2] +=   '</div>';
                if(slideCount>1){
                    homeAdd[2] +=   '<a id="left_arrow" class="carousel-control left" href="#mainCarousel" data-slide="prev" tabindex="-1">&lsaquo;</a>'+
                                    '<a id="right_arrow" class="carousel-control right" href="#mainCarousel" data-slide="next" tabindex="-1">&rsaquo;</a>';
                }
                homeAdd[2] += '</div>'+
                            '<div id="caption-space"></div>' +
                            '</div>';
                
console.warn(homeDesign);		
                switch (homeDesign){
                    case '1':
						$('#content2-2').attr('class','stl-content2 well col-sm-12 col-md-12');
						$('#content2-1').attr('class','col-sm-12 col-md-12');
                        $('#block_1').html(homeAdd[1]);
                        $('#block_2_1').html(homeAdd[2]);
                        $('#block_2_2').attr('class','col-sm-4 col-md-4 expander');
                        $('#block_2_2').html(homeAdd[3]).expander({ slicePoint: 450, expandText: ' Read more', userCollapseText: ' Read Less' });
                        $('#block_3_1').html(homeAdd[4]);
                        $('#block_3_2').html(homeAdd[5]);
                        break;
                    case '2':
						$('#content2-2').attr('class','stl-content2 well col-sm-12 col-md-12');
						$('#content2-1').attr('class','col-sm-12 col-md-12');					
                        $('#block_1').html(homeAdd[1]);
                        $('#block_2_1').attr('class','col-sm-4 col-md-4 expander');
                        $('#block_2_1').html(homeAdd[3]).expander({ slicePoint: 450, expandText: ' Read more', userCollapseText: ' Read Less' });
                        $('#block_2_2').html(homeAdd[2]);
                        $('#block_3_1').html(homeAdd[5]);
                        $('#block_3_2').html(homeAdd[4]);
                        break;
                    case '3':
						$('#content2-1').attr('class','stl-content2 well col-sm-12 col-md-12');
						$('#content2-2').attr('class','col-sm-12 col-md-12');					
                        document.getElementById('additionalSpace').style.display = '';
                        $('#block_1_1').html(homeAdd[2]);
                        $('#block_1_2').attr('class','col-sm-4 col-md-4 expander');
                        $('#block_1_2').html(homeAdd[3]).expander({ slicePoint: 450, expandText: ' Read more', userCollapseText: ' Read Less' });
                        $('#block_2').html(homeAdd[1]);
                        $('#block_3_1').html(homeAdd[4]);
                        $('#block_3_2').html(homeAdd[5]);
                        break;
                    case '4':
						$('#content2-1').attr('class','stl-content2 well col-sm-12 col-md-12');
						$('#content2-2').attr('class','col-sm-12 col-md-12');						
                        document.getElementById('additionalSpace').style.display = '';
                        $('#block_1_1').attr('class','col-sm-4 col-md-4 expander');
                        $('#block_1_1').html(homeAdd[3]).expander({ slicePoint: 450, expandText: ' Read more', userCollapseText: ' Read Less' });
                        $('#block_1_2').html(homeAdd[2]);
                        $('#block_2').html(homeAdd[1]);
                        $('#block_3_1').html(homeAdd[5]);
                        $('#block_3_2').html(homeAdd[4]);
                        break;
                    default:
                        $("#block_2_1").html('<div class="col-sm-7 col-md-7 expand"><h4 class="text-center">Home it\'s coming soon...</h4></div>');
                        break;
                }

                jQuery(function ($) {
                    $('.carousel').carousel();
                    var caption = $('div.item:nth-child(1) .carousel-caption'); //unsure about this
                    var text = $('div.item:nth-child(1) .carousel-caption p');
                    caption.css('text-align','center');
                    text.css({'font-weight':'bold','text-align':'center'});
                    $('#caption-space').html('');
                    $('#caption-space').html(caption.html());
                    caption.css('display', 'none');

                    $(".carousel").on('slide.bs.carousel', function (evt) {
                        var caption = $('div.item:nth-child(' + ($(evt.relatedTarget).index() + 1) + ') .carousel-caption');
                        var text = $('div.item:nth-child(' + ($(evt.relatedTarget).index() + 1) + ') .carousel-caption p');
                        caption.css('text-align','center');
                        text.css({'font-weight':'bold','text-align':'center'});
                        console.log(caption.html());
                        console.log(text.html());
                        $('#caption-space').html('');
                        $('#caption-space').html(caption.html());
                        caption.css('display', 'none');
                    });
                });
				
				$('#rss-blog').yrss(rssURL, {
					ssl: false,
