<div id="search-aux" style="display: none"></div>
<script>
$(document).ready(function(){
    $('#frmsrch').on('submit', function(e){
        e.preventDefault();
		var sstr = $('#keyword').val();
		$('#search-aux').text(sstr);
        $('#keyword').val( $('#search-aux').html() );
        this.submit();
    });
});
