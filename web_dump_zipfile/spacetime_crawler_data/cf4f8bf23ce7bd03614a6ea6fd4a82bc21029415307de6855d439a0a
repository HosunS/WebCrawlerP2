        var isOpera = (!!window.opr && !!opr.addons) || !!window.opera || navigator.userAgent.indexOf(' OPR/') >= 0;
        // Firefox 1.0+
        var isFirefox = typeof InstallTrigger !== 'undefined';
        // Safari 3.0+ "[object HTMLElementConstructor]" 
        var isSafari = Object.prototype.toString.call(window.HTMLElement).indexOf('Constructor') > 0 || (function (p) { return p.toString() === "[object SafariRemoteNotification]"; })(!window['safari'] || safari.pushNotification);
        // Internet Explorer 6-11
        var isIE = /*@cc_on!@*[quitame]/false || !!document.documentMode;
        // Edge 20+
        var isEdge = !isIE && !!window.StyleMedia;
        // Chrome 1+
        var isChrome = !!window.chrome && !!window.chrome.webstore;
        // Blink engine detection
        var isBlink = (isChrome || isOpera) && !!window.CSS;        
        
        */
        browser.mozilla = typeof InstallTrigger !== 'undefined';
        initSlideCount();
        
        
        if (browser.mozilla){
            $('.carousel').each(function(){
                $(this).carousel({
                    interval: false
                });
            });
        }
        
        $('#mainCarousel').bind('slid', function() {
            currentIndex = $('div.active').index() + 1;
            if (player.getPlayerState() == 1 && currentIndex != 2){
                player.pauseVideo();
                if (!browser.mozilla){
                    $('#mainCarousel').carousel('cycle');
                }
            }
            if (player2.getPlayerState() == 1 && currentIndex != 3){
                player2.pauseVideo();
                if (!browser.mozilla){
                    $('#mainCarousel').carousel('cycle');
                }
            }
        });

    });

    $(window).resize(function(){
        if ($('#mainCarousel').width() == 414){
            
            if ($('div.active').index() == 1){
                $('.carousel-caption').css('position', 'absolute', 'margin-top', '500px');
            }
        }
		
    });

