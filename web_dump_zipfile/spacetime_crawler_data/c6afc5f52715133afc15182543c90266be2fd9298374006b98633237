                    function getTextNodesIn(node, includeWhitespaceNodes) {
                      var textNodes = [], nonWhitespaceMatcher = /\S/;

                      function getTextNodes(node) {
                        if (node.nodeType == 3) {
                          if (includeWhitespaceNodes || nonWhitespaceMatcher.test(node.nodeValue)) {
                            textNodes.push(node);
                          }
                        } else {
                          for (var i = 0, len = node.childNodes.length; i < len; ++i) {
                            getTextNodes(node.childNodes[i]);
                          }
                        }
                      }

                      getTextNodes(node);
                      return textNodes;
                    }

                    
                    var markDownNodes = [];
                    $('.js-markdown').each(function(i,node) {
                      markDownNodes = markDownNodes.concat(getTextNodesIn(node,false));
                    });

                    
                    jQuery.each(markDownNodes, function(i, el) {
                      var oldtxt = $(el).text();
                      $(el).parent().html(marked(oldtxt));
                    });
                    $('.js-markdown table').addClass('table');
