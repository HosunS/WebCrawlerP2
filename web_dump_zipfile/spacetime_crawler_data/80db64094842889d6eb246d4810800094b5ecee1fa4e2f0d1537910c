        var orientation = window.orientation;
        switch (orientation) {
            case 0:
                window.location.reload();
                break;
            case 90:
                window.location.reload();
                break;
            case -90:
                window.location.reload();
                break;
        }
    };
    setTimeout(function() {

        // webmd.p.fit.samsung = /Galaxy.*Tab|SAMSUNG.*Tablet|Galaxy.*Tab|Android.*GT-/i;
