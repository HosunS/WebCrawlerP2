                 filebrowserBrowseUrl: 'http://cbcl.ics.uci.edu/lib/plugins/ckgedit/fckeditor/editor/filemanager/browser/default/browser.html?Type=File&Connector=http://cbcl.ics.uci.edu/lib/plugins/ckgedit/fckeditor/editor/filemanager/connectors/php/connector.php',                                
               } 
