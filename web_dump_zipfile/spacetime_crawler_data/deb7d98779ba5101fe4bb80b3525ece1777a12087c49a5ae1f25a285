<script>
window.onload = function myFunction() {
	var firstlabel=document.getElementById("label1").offsetHeight;
	var firsttitle=document.getElementById("title1").offsetHeight;
	var firstdesc=document.getElementById("desc1").offsetHeight;
	var firstprice=document.getElementById("price1").offsetHeight;
	if(document.getElementById("price1member"))
	{
		var firstpricemember=document.getElementById("price1member").offsetHeight;
	}
	var firstpricenonmember=document.getElementById("price1nonmember").offsetHeight;
	var secondlabel=document.getElementById("label2").offsetHeight;
	var secondtitle=document.getElementById("title2").offsetHeight;
	var seconddesc=document.getElementById("desc2").offsetHeight;
	var secondprice=document.getElementById("price2").offsetHeight;
	if(document.getElementById("price2member"))
	{
		var secondpricemember=document.getElementById("price2member").offsetHeight;
	}
	var secondpricenonmember=document.getElementById("price2nonmember").offsetHeight;
	var thirdlabel=document.getElementById("label3").offsetHeight;
	var thirdtitle=document.getElementById("title3").offsetHeight;
	var thirddesc=document.getElementById("desc3").offsetHeight;
	var thirdprice=document.getElementById("price3").offsetHeight;
	if(document.getElementById("price3member"))
	{
		var thirdpricemember=document.getElementById("price3member").offsetHeight;
	}
	var thirdpricenonmember=document.getElementById("price3nonmember").offsetHeight;
	
	var final_label = Math.max(firstlabel,secondlabel,thirdlabel);
	var final_title = (Math.max(firsttitle,secondtitle,thirdtitle)-20);
	var final_desc = Math.max(firstdesc,seconddesc,thirddesc);
	var final_price = Math.max(firstprice,secondprice,thirdprice);
	var final_pricemember = Math.max(firstpricemember,secondpricemember,thirdpricemember);
	var final_pricenonmember = Math.max(firstpricenonmember,secondpricenonmember,thirdpricenonmember);
	
	document.getElementById("label1").style.minHeight = final_label+"px";
	document.getElementById("title1").style.minHeight = final_title+"px";
	document.getElementById("desc1").style.minHeight = final_desc+"px";
	document.getElementById("price1").style.minHeight = final_price+"px";
	if(document.getElementById("price1member"))
	{
		document.getElementById("price1member").style.minHeight = final_pricemember+"px";
	}
	document.getElementById("price1nonmember").style.minHeight = final_pricenonmember+"px";
	document.getElementById("label2").style.minHeight = final_label+"px";
	document.getElementById("title2").style.minHeight = final_title+"px";
	document.getElementById("desc2").style.minHeight = final_desc+"px";
	document.getElementById("price2").style.minHeight = final_price+"px";
	if(document.getElementById("price2member"))
	{
		document.getElementById("price2member").style.minHeight = final_pricemember+"px";
	}
	document.getElementById("price2nonmember").style.minHeight = final_pricenonmember+"px";
	document.getElementById("label3").style.minHeight = final_label+"px";
	document.getElementById("title3").style.minHeight = final_title+"px";
	document.getElementById("desc3").style.minHeight = final_desc+"px";
	document.getElementById("price3").style.minHeight = final_price+"px";
	if(document.getElementById("price3member"))
	{
		document.getElementById("price3member").style.minHeight = final_pricemember+"px";
	}
	document.getElementById("price3nonmember").style.minHeight = final_pricenonmember+"px";
}
</script>

