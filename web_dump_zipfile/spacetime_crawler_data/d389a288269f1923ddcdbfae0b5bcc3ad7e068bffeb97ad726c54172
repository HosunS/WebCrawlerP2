#  Comparing two proportions
model
{
y1 ~ dbin(theta1,n1)
y2 ~ dbin(theta2,n2)
theta1 ~ dbeta(1,1)
theta2 ~ dbeta(1,1)
RR <- theta1/theta2
HT <- step(RR-1)
}
