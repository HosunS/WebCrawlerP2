<script type="text/javascript">
  /**
  * GDPR Toast functionality
  */
  (function() {
  var TOAST_FLAG = 'wu.gdpr-toast-viewed';
  var DSX_API_QUERY = 'q=assetName:$in(%27/' + window.dprManager.locale +
  '/lite-msg%27);tags.keyword:$in(%27' + window.dprManager.appId +
  '%27);tags.keyword:$in(%27gdpr%27)';
  var showPrivacyPolicyToast = window.localStorage.getItem(TOAST_FLAG);
  if (showPrivacyPolicyToast !== 'true') {
  var body = document.querySelector('body');
  if (!body) {
  return;
  }
  var toast = document.createElement('div');
  toast.id = 'privacy-policy-toast';
  body.insertBefore(toast, body.firstChild);
  var url = 'https://dsx.weather.com/cms/v4/assets/(' +
  window.dprManager.locale.replace("-", "_") +
  ')?api=7bb1c920-7027-4289-9c96-ae5e263980bc&' + DSX_API_QUERY;
  var request = new XMLHttpRequest();
  request.open('GET', url, true);
  request.onload = function() {
  if (request.status >= 200 && request.status < 400) {
          window.toast = window.toast || {};
          window.toast.close = function() {
            toast.style.display = 'none';
            window.localStorage.setItem(TOAST_FLAG, 'true');
          };
          var dataParse = JSON.parse(request.responseText);
          var toastBody = '';
          if (dataParse && dataParse[0] && dataParse[0].doc && dataParse[0].doc[0] && dataParse[0].doc[0].body) {
            toastBody = dataParse[0].doc[0].body;
          }
          toast.innerHTML = toastBody;
        }

      };
      request.send();
    }

  })();
</script>
