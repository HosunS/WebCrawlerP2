	<script type="text/javascript">
		function getOmnitureValue(value) {
		var dprObject = JSON.parse(window.localStorage.getItem('dpr/consents') || '{}');
		if (!!(dprObject && dprObject.featureFlags && dprObject.featureFlags.allowOmniture)) {
		return value;
		}
		return "";
		}
		function blockOmnitureValue(value) {
		var dprObject = JSON.parse(window.localStorage.getItem('dpr/consents') || '{}');
		if (!!(dprObject && dprObject.featureFlags && dprObject.featureFlags.allowOmniture)) {
		return "";
		}
		return value;
		}
		function getConsentOptIns() {
		var dprObject = JSON.parse(window.localStorage.getItem('dpr/consents') || '{}');
		if (dprObject && dprObject.consentOptIns) {
		return dprObject.consentOptIns;
		}
		return "";
		}
		function getCookie(name) {
		var value = null;
		var cookies = document.cookie ? document.cookie.split(";") : [];
		cookies.forEach(function(cookie) {
		if (cookie) {
		var cookieParts = cookie.split("=");
		var cookieName = cookieParts[0].trim();
		if (cookieName === name) {
		// Handles multi value cookies.
		var cookieValue = cookieParts.slice(1).join("=");
		value = cookieValue;
		}
		}
		});
		return value;
		}
		s.pageName = "MonthlyWeather - Yesterday: No DFP (Pages/WeatherStation/MonthlyWeather.cst)";
