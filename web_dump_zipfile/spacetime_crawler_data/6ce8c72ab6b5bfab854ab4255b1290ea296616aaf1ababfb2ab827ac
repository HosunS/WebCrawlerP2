<script src="//www.wunderground.com/static/scripts/dpr-sdk/1.4.16/index.js"></script>
<script type="text/javascript">
  (function() {
  var dprSettings = {};
  // Callback handlers.
  function getPrivacyRegime() {
  return new Promise(function(resolve, reject) {
  // Defaults regime to "exempt".
  var regime = 'exempt';
  // Try to get the regime from the ci cookie.
  if (parsedCiCookie && parsedCiCookie['TWC-Privacy']) {
  regime = parsedCiCookie['TWC-Privacy'];
  }
  resolve(regime);
  });
  }
  function getUserId() {
  return isUserLoggedIn() ? getAuthUserId() : getAnonUserId();
  }
  function isUserLoggedIn() {
  var emailAuth = getCookie('EmailAuth');
  var emailCookie = getCookie('EmailCookie');
  if (!emailAuth || !emailCookie) {
  return false;
  }
  if (emailAuth === '' || emailAuth !== emailCookie) {
  return false;
  }
  return true;
  }
  function getAnonUserId() {
  removeDataLocally('wu.userId');
  var userId = getDataLocally('wu.anonId');
  var userIdReady;
  if (!userId) {
  userIdReady = new Promise(function(resolve, reject) {
  userId = uuidGenenerator();
  storeDataLocally('wu.anonId', userId);
  resolve(userId);
  });
  }
  else {
  userIdReady = Promise.resolve(userId);
  }
  return userIdReady;
  }
  function getAuthUserId() {
  var userId = getDataLocally('wu.userId');
  if (!userId) {
  userIdReady = new Promise(function(resolve, reject) {
  var upsProfile = 'https://profile.wunderground.com/p';
  var xhr = new XMLHttpRequest();
  xhr.open('GET', upsProfile, true);
  xhr.withCredentials = true;
  xhr.setRequestHeader('content-type', 'application/json');
  xhr.send();
  xhr.onreadystatechange = function() {
  if (this.readyState !== 4 || this.status !== 200) {
  resolve('auth_undefined');
  } else {
  var response = JSON.parse(xhr.responseText);
  userId = response.userId;
  storeDataLocally('wu.userId', userId);
  resolve(userId);
  }
  }
  });
  }
  else {
  var userIdReady = Promise.resolve(userId);
  }
  return userIdReady;
  }
  //from @twc/uuid package
  function uuidGenenerator(a) {
  return a
  ? // if a placeholder was passed, return a random number from 0 to 15
  // unless uuid is 8, in which case return a random number from 8 to 11 in
  // hexadecimal
  (a ^ ((Math.random() * 16) >> (a / 4))).toString(16)
  : // or otherwise return a concatenated string:
  // 10000000 +
  // -1000 +
  // -4000 +
  // -8000 +
  // -10000000000,
  // replacing 0s with 1s and 8s with random hex
  ([1e7] + -1e3 + -4e3 + -8e3 + -1e11).replace(/[018]/g, uuidGenenerator);
  }
  function getGeoIPCountryCode() {
  return new Promise(function(resolve, reject) {
  var countryCode = '';
  // Try to get the country code from the ci cookie.
  if (parsedCiCookie && parsedCiCookie['TWC-GeoIP-Country']) {
  countryCode = parsedCiCookie['TWC-GeoIP-Country'];
  }
  resolve(countryCode);
  });
  }
  function getApplicationInfo() {
  return new Promise(function(resolve, reject) {
  const appData = {
  id: 'weather.com',
  version: '1.0.0',
  };
  resolve(appData);
  });
  }
  function getUserData() {
  return new Promise(function(resolve, reject) {
  resolve(getDataLocally("dpr/consents"));
  });
  }
  function setUserData(userData, featureFlags) {
  userData.featureFlags = featureFlags;
  setDprSettings(userData);
  return new Promise(function(resolve, reject) {
  storeDataLocally("dpr/consents", JSON.stringify(userData));
  resolve();
  });
  }
  function setDprSettings(userData) {
  dprSettings = userData.featureFlags;
  }
  function logUserConsent(message) {
  return new Promise(function(resolve, reject) {
  if (message && message.user && message.user.id === 'auth_undefined') {
  return resolve();
  }
  var sqsBeaconUrl = 'https://weather.com/api/v1/p/p/pmq';
  var request = createPostRequest(sqsBeaconUrl);
  var sqsBeaconPaylod = getSqsBeaconPaylod(message);
  request.onload = function() {
  if (request.status >= 200 && request.status < 400) {
            resolve();
          } else {
            reject();
          }
        };

        request.send(sqsBeaconPaylod);
      });
    }

    function storeDataLocally(key, value) {
      window.localStorage.setItem(key, value);
    }

    function getDataLocally(key) {
      var value = window.localStorage.getItem(key);
      try {
        value = JSON.parse(value);
      } catch (e) {}

      return value;
    }

    function removeDataLocally(key) {
      return window.localStorage.removeItem(key)
    }

    // Helpers.
    function parseCiCookie() {
      var ci = getCookie('ci_test') || getCookie("ci") || "";
      var parsedCiCookie = {};
      ci.split('&').forEach(function(data) {
        var dataParts = data.split('=');
        parsedCiCookie[dataParts[0]] = dataParts[1];
      });

      return parsedCiCookie;
    }

    function getCookie(cookieName) {
      var matches = document.cookie.match(new RegExp(
        "(?:^|; )" + cookieName.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
      ));
      return matches ? decodeURIComponent(matches[1]) : undefined;
    }

    function createPostRequest(url) {
      var shouldBeAsync = true;
      var request = new XMLHttpRequest();

      request.onload = function () {
        // Do we really want to do something in case of success or failure?
        // If so, put that here.
        console.log(request);
      }

      request.open('POST', url, shouldBeAsync);

      // request.setRequestHeader('Content-Type', 'application/json');
      // request.setRequestHeader('Cache-Control', 'no-cache');

      return request;
    }

    function getSqsBeaconPaylod(message) {
      // We need to fix this data here for now.
      var paylod = {
        type: 'sqs',
        config: {
          accessKey: 'AKIAIGA436NL5XWDIPTA',
          secretKey: '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',
          account: '747624766027',
          queue: 'events_profile_change_web'
        },
        doc: message
      };

      return JSON.stringify(paylod);
    }

    var parsedCiCookie = parseCiCookie();

    window.dprManager = window.dprManager || {};
    window.dprManager.locale = 'en-US';
    window.dprManager.appId = 'wu-web';

    if (window.dprManager.dpr) {
      window.dprManager.getCookie = getCookie;
      var sdkSettings = {
        getPrivacyRegime: getPrivacyRegime,
        getUserId: getUserId,
        getGeoIPCountryCode: getGeoIPCountryCode,
        getApplicationInfo: getApplicationInfo,
        getStoredUserData: getUserData,
        setStoredUserData: setUserData,
        logUserConsent: logUserConsent,
        // getConsentConfig: getConsentConfig
      }
      var dprReady = new Promise(function(resolve, reject) {
        window.dprManager.dpr(sdkSettings).then(function() {
          return resolve(dprSettings);
        });
      });
      var dprShow = window.dprManager.openConsentManager || function() {};
    }
    else {
      // Use an auto-resolved promise with default dpr settings.
      var defaultDprSettings = {
        allowAGOF: true,
        allowBurdaAdEngine: true,
        allowBurdaHeaderBidders: true,
        allowColombia: true,
        allowComscore: true,
        allowFindMe: true,
        allowGPT: true,
        allowGPTPii: true,
        allowGeoIP: true,
        allowGoogleAnalytics: true,
        allowHeaderBidders: true,
        allowLigatus: true,
        allowMarketingPixel: true,
        allowNielsen: true,
        allowOCW: false,
        allowOutbrain: true,
        allowPrismaGPT: true,
        allowRogersAdUtil: true,
        allowSocialLinksFooter: false,
        allowSocialSharing: false,
        allowTaboola: true,
        allowTwitterFeed: false,
        allowUPS: false,
        type: "default",
        useHeaderBidderWhitelist: false
      };
      var dprReady = Promise.resolve(defaultDprSettings);
      var dprShow = function() {};
    }

    // Share in window the dpr ready promise so other stuff consuming data
    // set by the dpr process will know when it is safe to start its logic.
    window.dprReady = dprReady;
    window.dprShow = dprShow;

  })();
</script>
<script type="text/javascript">
  var privacyPreferencesLinkArr = document.getElementsByClassName("privacy-preferences-link");
  var privacyPolicyContent = document.querySelector(".privacy-policy-content");
  if(window.dprReady) {
  window.dprReady
  .then(
  function(dprSettings) {
  if (dprSettings.type !== 'exempt') {
  for (var i=0; i < privacyPreferencesLinkArr.length; i++) {
              privacyPreferencesLinkArr[i].style.display = 'block';
              privacyPreferencesLinkArr[i].addEventListener("click", function (event) {
                event.preventDefault();
                if (window.dprShow) {
                  window.dprShow();
                }
              })
            }
            if (privacyPolicyContent) {
              privacyPolicyContent.style.display = 'block';
            }
          }
        }
      )
  }

</script>
