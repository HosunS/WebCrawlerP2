<script type="text/template" id="favorites-city-template">
	<%
		var options = options || {};
		var isCity = true;
		var isFav = wui.favorites.is(city.zmw);
		var isHome = ''==city.zmw?true:false;
		//hasConds: (options.showConditions && !!(item.condition))
		var hasConds = !!(city.condition);
		var defcon = hasConds && city.condition.defcon ? city.condition.defcon : 5;
	%>
	<% if (city.zmw) { %>
		<li class="<% if (isCity) { %>is-city<% } %><% if (isFav) { %> is-fav<% } %><% if (hasConds && city.condition.severe) { %> is-severe defcon-<%= defcon %><% } %><% if (hasConds) { %> has-conds<% } %>" <% if (isCity) { %>data-zmw="<%= city.zmw %>" data-cityName="<%= city.name %>" <% } %>>
		<% if (options.label) { %><label><%= options.label %></label><% } %>
		<a href="<%= city.url %><% if (city.zip != 00000) { %>/<%= city.zip %><% } %>">
		<span class="city-name"><%= city.name %> <i class="fi-<% if (defcon > 4) { %>info<% } else { %>alert<% } %>"></i></span>
		<% if (hasConds) { %>
		<span class="city-conds">
		<%= city.condition.temperature %> &deg;F <%= city.condition.condition %>
		</span>
		<span class="cond-icon-touch cond-set-<%= city.condition.iconPref %> cond-<%= city.condition.icon %>"></span>
		<% } %>
		<% if (isCity) { %><span class="city-star" title="<% if (isFav) { %>Remove From<% } else { %>Add To<% } %> Favorite Cities"></span><% } %>
		<%
					//removing home button from list now that we have location management on the page - dxf
					if (isCity && 0) {
				%><span class="city-home <% if (isHome) { %> active <% }  %>" title="Set this as your Home City"</span><% } %>
	 </a>
		</li>
	<% } %>
</script>
