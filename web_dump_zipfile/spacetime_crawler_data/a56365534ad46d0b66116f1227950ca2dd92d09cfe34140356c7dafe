		<script type="text/javascript">
		/*<![CDATA[*/

			function newWindow (newLocation, width, height) {
				var popUp = window.open(newLocation, '', 'width='+width+',height='+height+',screenX=20,screenY=20,toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=yes,copyhistory=no');
				popUp.focus();
			};

			function log(message) {
				if (typeof (console) != 'undefined' && window.console) {
					console.log(message);
				} 
			};

			var imageFirst = function(article) {

				// Get the first image
				var image = $(article).find('img').first();

				if(image.length < 1) { return; }

				var detachedImage = $(image).detach();

				// Wrap the other content
				$(article).wrapInner("<div class='content' />");

				// Create wrapper link to article
				var link = $('<a href="' + $(article).attr("data-href") + '" />');
			
                $(detachedImage).prependTo(link);

				// Add to beginning of article
				$(link).prependTo(article);

                article.find("p:empty").remove();
			};

			var isViewType = function(theType) {
				return $('body').attr("class").indexOf("type-" + theType) != -1;
			};

			var changeReadMoreText = function(article) {
				$('.type-tag article').find('a.read_more').text("View Full Story");
			};

            var sizeUpVideo = function(video) {
                var aspectRatio = $(video).attr("width") / $(video).attr("height");
                $(video).attr("width", "100%");
                var newVideoHeight = $(video).width() / aspectRatio;
                $(video).attr("height", newVideoHeight);
            };

			var setMenuCurrentPage = function(tagName) {
				$('nav [data-name="'+ tagName +'"]').addClass('current');
			};

			var fancyShare = function(shareSections) {
				shareSections.find('a').click(function(e) {
					e.preventDefault();
					newWindow($(this).attr("href"), 600,600);
				});	
			};

            var adjustForPage = function() {
				$('body').attr("class", "type-page");
				$('header').hide();
                imageFirst($('article'));
            };

			// ******** Main *********
			(function() {

				// If not "permalink" style, switch the image to beginning of post
				if(!isViewType("permalink")) {
					$.each($('.posts article'), function(i, article) {
						//imageFirst(article);
						changeReadMoreText(article);
					});
				}

               // Functions for Detail Page
               if(isViewType("permalink")) {
                    // Make videos bigger on permalink page
                    $('article.post-video iframe').each(function() {
                        sizeUpVideo(this);
                    });

                    var altSrc = $('article [data-alt-src]').attr('data-alt-src');
                    if(altSrc) {
                        $('article img').first().attr("src", altSrc);
                    }
				}

				// Functions for Tag Page
				

                // Setup for Pages
                if(location.pathname == "/about") {
                    adjustForPage();
                }

				// Functions for all pages
				fancyShare($('.share'));

			})();

		/*]]>*/
		</script>

