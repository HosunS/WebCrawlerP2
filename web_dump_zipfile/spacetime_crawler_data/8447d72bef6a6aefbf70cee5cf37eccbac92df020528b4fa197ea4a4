      });
    });

    $(window).load(function() {
      // Must hide this with JS. If hidden using CSS, box-size JS calculates width as zero
      $('#same_as_products').hide();
    });



		$('div.scope, div.purpose, div.third-party-content').expander({
			slicePoint: 500, 
			detailClass: 'expanded_detail',
			expandEffect: 'fadeIn',
			collapseEffect: 'fadeOut'
		});

//]]>
</script>

