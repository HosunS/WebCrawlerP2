    $(document).on("submit", "#navMenu-searchNav", function(e) {
      e.preventDefault();
      var val = $(this).find("input.search-input").val();
      // Get rid of "."
      val = val.replace(/^\.$/, "");
      // Get rid of ".."
      val = val.replace(/^\.\.$/, "");
      // Get rid of every "%"
      val = val.replace(/%/g, "");
      val = val.trim();
      if (val !== '') {
        location.replace("/cms/render/live/en/sites/isoorg/search.html?q=" + val);
      }
    });
  </script>


