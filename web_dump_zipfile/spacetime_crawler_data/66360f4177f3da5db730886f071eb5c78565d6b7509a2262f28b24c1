    <script>
        var isMarketplacePopUpOpen;

        function togglePopUpContent() {
            if (isMarketplacePopUpOpen == true) {
                isMarketplacePopUpOpen = false;
                markMarketplaceSubmenuClosed();
                toggleMarketplaceIndicatorBoard('hide');
                enableHoverSubmenus();
            }
            else {
                isMarketplacePopUpOpen = true;
                markMarketplaceSubmenuOpened();
                toggleMarketplaceIndicatorBoard('show');
                disableHoverSubmenus();
            }
        }

        function markMarketplaceSubmenuClosed() {
            jQuery("#marketplace_visibility_indicator_arrow").attr("src", "http://g-ec2.images-amazon.com/images/G/01/Audible/en_US/images/background/arrow-up._V189891672_.png");
            jQuery("#marketplace_name_nav_display_font").attr("color", "orange");
        }

        function markMarketplaceSubmenuOpened() {
            jQuery("#marketplace_visibility_indicator_arrow").attr("src", "http://g-ec2.images-amazon.com/images/G/01/AmazonMobileApps/arrow-down._V397964062_.png");
            jQuery("#marketplace_name_nav_display_font").attr("color", "gray");
        }

        function changeMarketplace(newMarketplace) {
            var expires = new Date();
            //expire after 2 years
            expires.setDate(expires.getDate() + (365*2));
            document.cookie = "mm-ssm" + "=" + newMarketplace + "; expires=" + expires.toUTCString() + "; path=/; domain=" + "advantage.amazon.com";
            location.reload(true);
        }

        function getMenuItemId(category) {
            return (category + "_hoverover_nav_pop");
        }

        function getSubmenuItemId(category) {
            return (category + "_hoverover_nav_content");
        }

        function enableHoverSubmenu(category) {
            var menuItemIdString = "#" + getMenuItemId(category);
            var submenuIdString = "#" + getSubmenuItemId(category);
            jQuery(menuItemIdString).removeAmazonPopoverTrigger();

            var isSelectedItem = (category == '');
            if (isSelectedItem == true) {
                jQuery(document).ready(function() {
                    amznJQ.available('popover', function() {
                                  jQuery(menuItemIdString).amazonPopoverTrigger({
                                      showOnHover: true,
                                      localContent: jQuery(submenuIdString),
                                      showCloseButton: false,
                                      skin: '',
                                      width: 0,
                                      location: 'bottom',
                                      align: 'left',
                                      forceAlignment: true,
                                      hoverShowDelay: 25,
                                      hoverHideDelay: 25,
                                      followLink: true,

                                      locationMargin: 0
                              });
                    });
                });
            }
            else {
                jQuery(document).ready(function() {
                    amznJQ.available('popover', function() {
                                  jQuery(menuItemIdString).amazonPopoverTrigger({
                                      showOnHover: true,
                                      localContent: jQuery(submenuIdString),
                                      showCloseButton: false,
                                      skin: '',
                                      width: 0,
                                      location: 'bottom',
                                      align: 'left',
                                      forceAlignment: true,
                                      hoverShowDelay: 25,
                                      hoverHideDelay: 25,
                                      followLink: true,

                                      onShow: function() {
                                         jQuery(menuItemIdString).removeClass("tab-text-not-highlighted").addClass("tab-text-highlighted");
                                      },
                                      onHide: function() {
                                         jQuery(menuItemIdString).removeClass("tab-text-highlighted").addClass("tab-text-not-highlighted");
                                      },

                                      locationMargin: 0
                                  });
                    });
                });
            }
        }

        function disableHoverSubmenu(category) {
            var menuItemIdString = '#' + getMenuItemId(category);
            jQuery(menuItemIdString).removeAmazonPopoverTrigger();
        }

        function enableHoverSubmenus() {
        }

        function disableHoverSubmenus() {
        }

        function toggleMarketplaceIndicatorBoard(command) {
            var menuItemIdString = "#marketplace-indicator_click_nav_pop";
            var submenuIdString = "#marketplace-indicator_click_nav_content";

            if (command == 'show') {
                jQuery(submenuIdString).show();
            }
            else {
                jQuery(submenuIdString).hide();
            }
        }

        jQuery(document).ready(function() {
            amznJQ.available('popover', function() {
                                  jQuery("#marketplace-indicator_click_nav_pop").amazonPopoverTrigger({
                                      localContent: jQuery("#marketplace-indicator_click_nav_content"),
                                      showCloseButton: true,
                                      skin: '',
                                      width: 0,
                                      location: 'bottom',
                                      align: 'left',
                                      forceAlignment: true,
                                      locationMargin: 0
                                  });
            });
        });

        enableHoverSubmenus();
    </script>






















