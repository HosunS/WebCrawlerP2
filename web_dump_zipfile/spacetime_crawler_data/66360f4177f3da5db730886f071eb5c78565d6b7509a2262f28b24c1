<script language="javascript">
    function checkPasswordsForAllSpaces() {
        var element = document.getElementById('password');
        var passwordWarning = document.getElementById('passwordWarning');

        if ( element == null || passwordWarning == null) return true;
        if ( element.value == null || element.value == "" ) {
            passwordWarning.style.display = 'none';
            return true;
        }

        var passwordValue = element.value;
        passwordValue = passwordValue.replace(/^\s+|\s+$/, '');
        if ( passwordValue == "" ) {
            passwordWarning.style.display = '';
            return false;
        } else {
            passwordWarning.style.display = 'none';
            return true;
        }

        return true;
    }
    
    function setHiddenEmail() {
    	var email = document.getElementById("email");
    	var emailCheck = document.getElementById("emailCheck");
    	if( email != null && emailCheck != null && email.value != null ) {
    	   emailCheck.value = email.value;	
    	}
    }

    function submitCreateAccountForm() {
        setHiddenEmail();
        if(checkPasswordsForAllSpaces()) {
            document.getElementById('create_account_form').submit();
        }
    }
    
    function enableDisableNextButton() {
            var fields = new Array("userName","email");
        var nonEmptyPattern = /\S/g ;
        for (var j = 0; j < fields.length; j++) {
    	    var value = document.getElementById(fields[j]).value;
    	    if(!value || !value.match(nonEmptyPattern) ){
                document.getElementById("nextButtonEnabled").style.display="none";
                document.getElementById("nextButtonDisabled").style.display="block";
                return false;
            }
        }
        document.getElementById("nextButtonDisabled").style.display="none";
        document.getElementById("nextButtonEnabled").style.display="block";
        return true;
    }
    
</script>

