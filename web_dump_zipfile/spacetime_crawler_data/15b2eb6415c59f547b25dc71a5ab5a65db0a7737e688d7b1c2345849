   <define name="Lattice">
      <!-- lattice content model -->
      <element name="lattice">
         <optional>
            <ref name="Time-ref-anchor-point"/>
         </optional>
         <optional>
            <ref name="Time-ref-uri"/>
         </optional>
         <attribute name="final">
