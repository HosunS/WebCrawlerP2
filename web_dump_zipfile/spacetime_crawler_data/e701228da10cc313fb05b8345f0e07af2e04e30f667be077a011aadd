	function facebook_connect() {
		if (typeof FB == "undefined") {
			return
		}

		FB.login(function(response) {
			if (!response.authResponse) {
				return;
			}
			var p = qs_param();
			if (p["from"]) {
				var from = p["from"][0];
				location.href = "/facebook::post_login?from="+encodeURIComponent(from);
			} else {
				location.href = "/facebook::post_login";
			}
			//facebook_login(response);
		}, {scope:'email'});
	}

	var facebookPostInit = _facebookPostInit;



