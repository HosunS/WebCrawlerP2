<script>
$(function () {
	$('a').on('click touchend', function (e) {
		var href = $(this).attr('href');

		if (/https?:\/\/www\.(?:preview\.|staging\.)?webmd\.com/.test(href)) {
			e.preventDefault();
			webmd.offsite.showInterstitial('/click?url=' + href);
		} else {
			webmd.offsite.showInterstitial(href);
		}

		$('.overlay-btn').on('click', function () {
			webmd.overlay.close();
		});
	});
});
</script>

