<script>
    amznJQ.onReady('AuthorCentralCommon', function() {
        $('#email').focus();

        $('form#signinWidget').submitOnEnter()
            .find('input[name=path]').val('').end()
            .find('input:visible:first').focus();

        $('#joinNow').click(function() {
             $('form#signinWidget input[name=path]').val('/gp/join');
             $('form#signinWidget').submit();
             return true;
        });
    });

    amznJQ.onReady('AuthorCentralCommon', function() {
        var selectedPage = 1;

        /* Auto-rotate pages */
        var flipper = setInterval(function() {
            switchPage(selectedPage == 3 ? 1 : selectedPage + 1, 1500);
        }, 7000);

        /* Select page */
        function switchPage(page, speed) {
            if (selectedPage == page)
              return;

            $('#splashBox ul.nav li').removeClass('active');
            $('#splashBox ul.nav li:nth-child('+page+')').addClass('active');
            if ($.browser.msie && parseInt($.browser.version) <= 8) {
              $("#slide"+selectedPage).hide();
            } else {
              $("#slide"+selectedPage).fadeOut(speed);
            }
            $("#slide"+page).fadeIn(speed);

            selectedPage = page;
        }

        /* Init pages */
        var pageNum = 1;
        $('#splashBox ul.nav li').each(function() {
            $(this).data('page', pageNum++).click(function() {
                clearInterval(flipper);
                switchPage($(this).data('page'), 250);
            });
        });

    });
</script>

