#ifndef _looptimer_h_
#define _looptimer_h_


#include <stdio.h>
#include <time.h>
#include <string>

using namespace std;

class LoopTimer
{
private:
    string desc;
    unsigned count;
    unsigned step;
    struct timespec startTime;
    struct timespec prevTime;
    struct timespec currTime;
    struct timespec sumTime;
    bool running;

    void print();
    
public:
    FILE* file;
    
    LoopTimer();
    void begin(const string &desc, unsigned count);
    void next(unsigned step = 1, unsigned count = 0);
    void pause();
    void resume();
    void end();
    unsigned totalTime();
    float avgTime();
};

#endif

