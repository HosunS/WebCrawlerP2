                        <span style="color:#ffcc66;">B</span>
                        <span style="color:#d7e972;">l</span>
                        <span style="color:#e45c5c;">o</span>
                        <span style="color:#d7e972;">c</span>
                        <span style="color:#76e2c2;">k</span>
                        <span style="color:#5dc6cd;">q</span>
                        <span style="color:#be7ce4;">u</span>
                        <span style="color:#e45c5c;">o</span>
                        <span style="color:#ffcc66;">t</span>
                        <span style="color:#d7e972;">e</span>
                        <span style="color:#e45c5c;">s</span>
                        &nbsp;
                        <span style="color:#d7e972;">T</span>
                        <span style="color:#76e2c2;">u</span>
                        <span style="color:#5dc6cd;">t</span>
