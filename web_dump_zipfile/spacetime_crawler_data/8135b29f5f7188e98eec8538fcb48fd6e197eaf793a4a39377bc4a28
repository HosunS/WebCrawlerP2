# 10 blocks (levels 1 to 10 with higher levels corresponding to more professional experience
# 3 treatments = training programs (train at home, train at a local regional center, train at national headquarters
# response is score on a final exam

# load library and read in data
library(tidyverse)
datainput <- read_csv("H://HAL/Courses/Stat210//auditortraining.csv")
training <- data.frame(datainput)
training$Experience <- as.factor(training$Experience)
# plot data with each block in a different color; connect points with lines
ggplot() +
  geom_point(data=training, mapping=aes(x=Method, y=Score, color=Experience)) + 
  geom_line(data=training, mapping=aes(x=Method, y=Score, color=Experience, group=Experience)) +
  scale_x_discrete(name="Treatment",labels=c("Home","Local","National")) +
  ylab("Score")
# fit randomized block analysis of variance and obtain summary table
training_rndmblock <- aov(training$Score ~ training$Experience + training$Method)
summary(training_rndmblock)
# check assumptions - residuals vs treatment
ggplot() +
  geom_point(data=training, mapping=aes(x=Method, y=training_rndmblock$residuals), position = position_jitter(width = 0.1, height = 0.0)) 
tapply(training_rndmblock$residuals, training$Method, describe)
# check assumptions - residuals vs block
ggplot() +
  geom_point(data=training, mapping=aes(x=Experience, y=training_rndmblock$residuals), position = position_jitter(width=0.1))
tapply(training_rndmblock$residuals, training$Experience, describe)
# check assumptions - normality
qqnorm(training_rndmblock$residuals)
# obtain treatment means for subsequent analysis 
tapply(training$Score, training$Method, describe)
