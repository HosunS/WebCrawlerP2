for(i in 1:n){
  FEV[i] ~ dnorm(mu[i],tau)
  mu[i] <- beta[1] + beta[2]*Age[i]   
  CPOinv[i] <- sqrt(2*3.14159/tau)*exp(0.5*tau*pow(FEV[i]-mu[i],2))
  }
beta[1] ~ dnorm(0,0.001) 
beta[2] ~ dnorm(0,0.001)
tau ~ dgamma(0.001,0.001)
