library(tidyverse)
datainput <- read_csv("H://HAL//Courses/Stat210/chicinsur.csv")
chicago <- data.frame(datainput)

apply(chicago,2,describe)
round(cor(chicago), 3)
plot(chicago$pctmin,chicago$newpol)
