<script type="text/javascript">
	$(document).ready(function () {
		showHideLanguagePanel();

		function showHideLanguagePanel() {
			//by default show language selection dropdown
			$("#divSelectLanguage").show();
			$("#divBackToOriginal").hide();

			var currentHref = location.href;
			//if not contain query string "&lt=" will show language selection panel
			if (currentHref.indexOf("&tl=") == -1) {
				$("#divSelectLanguage").show();
				$("#divBackToOriginal").hide();

			}
			else {//else show back to english panel
				$("#divSelectLanguage").hide();
				$("#divBackToOriginal").show();
			}

		}



		$("a.backToEnglish").click(function (e) {
			e.preventDefault();
			resetLang();
			return false;
		});
	});

	function resetLang() {
		var tmpURL = unescape(getQuerystring("u"));
		tmpURL = tmpURL.replace(/%3Ftranslate%3Doff/gi, "");
		tmpURL = tmpURL.replace(/\?translate=off/gi, "");
		top.location.href = tmpURL; // + "?translate=off";
	}

	function getQuerystring(key, default_) {
		if (default_ == null) default_ = "";
		key = key.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
		var regex = new RegExp("[\\?&]" + key + "=([^&#]*)");
		var qs = regex.exec(window.location.href);
		if (qs == null)
			return default_;
		else
			return qs[1];
	}
</script>

