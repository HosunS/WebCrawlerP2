<script type="text/javascript">
var today=new Date();

function updateClock()
{
    mins = today.getMinutes();
    hours = today.getHours();
    secs = today.getSeconds();

    if (document.clock)
        document.clock.time.value =  
            hours + (mins < 10 ? ":0" : ":") + mins +
            (secs < 10 ? ":0" : ":") + secs;
    setTimeout("updateClock()",100);
}

</script>
