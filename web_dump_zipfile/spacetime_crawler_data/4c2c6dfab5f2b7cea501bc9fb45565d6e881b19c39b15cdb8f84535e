    <script type="text/javascript"> 
    jQuery(document).ready(function($) {
        
        
        $('.next-slide').click(function(){
            $('#new-royalslider-2').data('royalSlider').next();
        });
         $('.slide-toggle').click(function(){
            var currentClass = $(this).data('toggle');
            if(currentClass == 'stop'){
                $('#new-royalslider-2').data('royalSlider').stopAutoPlay();
                $(this).data('toggle','play');
                $(this).removeClass('stop-button-green');
                $(this).addClass('play-button-green');
            }else{
                $('#new-royalslider-2').data('royalSlider').startAutoPlay();
                $(this).data('toggle','stop');
                $(this).removeClass('play-button-green');
                $(this).addClass('stop-button-green');
            }
        });
        $('.previous-slide').click(function(){
             $('#new-royalslider-2').data('royalSlider').prev();
        });
        jQuery( window ).resize(function() {
        	jQuery( "#menu-main > li > a " ).each(function( index ) {
        	  	jQuery(this).height("auto")
        	});
        	about =jQuery('#menu-item-908').height();
        	event =jQuery('#menu-item-1001').height();
        	if(about < event){
        		jQuery( "#menu-main > li > a " ).each(function( index ) {
        		  	jQuery(this).height(event)
        		});
        	}
        });
        
        about =jQuery('#menu-item-908').height();
    	event =jQuery('#menu-item-1001').height();
    	if(about < event){
    		jQuery( "#menu-main > li > a " ).each(function( index ) {
    		  	jQuery(this).height(event)
    		});
    	}
        
        
    });
    </script>
