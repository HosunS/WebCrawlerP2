  jQuery(function($) {
     if ($('#mobile-nav').is(':visible')) {
        var list = $('#navigation');
        var list_items = jQuery('>li', list);
        var $select = jQuery('<select></select>');
        var windowLocation = window.location.toString();
        var selectedValue = null;

        var $option = jQuery('<option></option>');
        $option.text('HOME');
        $option.val('/');
        $option.appendTo($select);

        list_items.each(function(){
          var text = jQuery('>a', this).text();
          var href = jQuery('>a', this).attr('href');

          if (windowLocation.indexOf(href, windowLocation.length - href.length) !== -1 || $(this).hasClass("active")) {
              selectedValue = href;
          }

          var $option = jQuery('<option></option>');
          $option.text(text);
          $option.val(href);
          $option.appendTo($select);

          if (!text.match(/brands/i)) {
            var sub_list_items = jQuery(this).find('li');
            sub_list_items.each(function(){
              var href = jQuery('>a', this).attr('href');

              if (windowLocation.indexOf(href, windowLocation.length - href.length) !== -1) {
                  selectedValue = href;
              }

              var $option = jQuery('<option></option>');
              $option.html('&nbsp;&nbsp;' + jQuery('>a', this).text());
              $option.val(href);
              $option.appendTo($select);
            });
          }
        });

        if (selectedValue !== null) {
          $select.val(selectedValue);
        }

        $select.change(function(){
          var href = jQuery(this).val();
          window.location.replace(href);
        });

        list.after($select);
        list.hide();
     }
  });
  </script>

