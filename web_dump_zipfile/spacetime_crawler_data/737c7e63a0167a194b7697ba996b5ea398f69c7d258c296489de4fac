<script type="text/javascript">
  function previewForm(theForm){
    theForm.previewField.value=document.MSketch.getMol("smiles");
  }  
  function submitSmiles(theForm){
    previewForm(theForm);
    var smiles = document.MSketch.getMol("smiles");
    
    if (smiles != null){
      opener.updateSmiles(window, smiles);
    }
  }
  function getInitialSmiles(){
    var elemName = opener.win_name_to_pop_id(window.name);
    var smiles = opener.document.getElementById(elemName).value;
    return smiles;
  }
  function setInitialSmiles(){
    var smiles = opener.getSmilesForWindow(window)
    if (document.MSketch != null && smiles != null && smiles != '' ){
      document.MSketch.setMol(smiles);
    }
  }
</script>


