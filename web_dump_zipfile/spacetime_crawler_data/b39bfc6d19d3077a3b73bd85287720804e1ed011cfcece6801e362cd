	function dealWithFirst(formIndex) {
		var form = document.forms[formIndex];
		// All this does is:
		// 		If they check the box, populate the form element "second" with all the departments NOT affiliated with a school...
		// 		If the *uncheck* the box, populate the form element "second" with all the departments for the currently selected school

		// users *check* the box
		if (form.supress_school.checked) {
			// Force "first" do display message...
			var options = form.first;
			var matched = false;
			for(var i=0; i < form.first.length; ++i) {
				if (options[i].value == "dont_display_school") {
					options.selectedIndex = i;
					matched = true;
				}
			}
			// ... then
			// 		if the user has *not* selected "supress_department" then make "second" display the departments w/o a school.
			if ( !form.supress_department.checked ) {
				populateSecond(formIndex);
			}

		// users *un-check* the box
		} else {
			// If they didn't select a school previously, force it to something
			if (form.first.value == "dont_display_school") {
				form.first.selectedIndex = 0;
			}

			// As long as the user hasn't selected "department not applicable", populate ""second"
			if ( !form.supress_department.checked) {
				changeSelect(form.first, formIndex);
			}
		}
	}

	function dealWithSecond(formIndex) {
		// users *check* the box, so we display a message that the department won't be used
		var form = document.forms[formIndex];

		if (form.supress_department.checked) {
			form.second.length = 1;
			form.second[0].value = "no-dept-selected";
			form.second[0].text = "(department not applicable)";

		// users *un-check* the box
		} else {
			// if "supress school" is selected, show departments w/o a school
			if ( form.supress_school.checked ) {
				populateSecond(formIndex);

			// If *a* school is selected in "first", display departments for that school
			} else {
				changeSelect(form.first, formIndex);
				// If *no* school is selected, display all department w/out a school
			}
		}

	}

	function populateSecond(formIndex){
		// replace this with a query...
		var optionArray = getDepartmentsWOSchool();

		document.forms[formIndex].second.length = optionArray.length;
		// Assign values to "second" select
		for(i=0; i < optionArray.length; ++i) {
			document.forms[formIndex].second[i].value = optionArray[i][0];
			document.forms[formIndex].second[i].text = optionArray[i][1];
		}
	    return true;
	}
 // -->
</script>

