<script>
function resetModule(){
  document.getElementById("mapContainder").style.backgroundImage= "url('/assets/region-news/regionsoff.jpg')";
  document.getElementById("mapLink").href= "/region-news";		
}

function getPosition(id) { //'alert(id);
   // e = e || window.event; alert(e.which);
   // var id = e.srcElement ? e.srcElement.id:e.which;alert(id);
    document.getElementById("mapContainder").style.backgroundImage= "url('/assets/region-news/region"+id+"-map.jpg')";
    var link = "/region-news" + (parseInt(id) <=6 ? '': '/region-'+id);   
    document.getElementById("mapLink").href= link;	
    return;   
}
</script>		

    

