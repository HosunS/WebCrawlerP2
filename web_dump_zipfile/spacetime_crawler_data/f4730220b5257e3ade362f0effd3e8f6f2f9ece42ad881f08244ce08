 function toggleAuthorKeys() {
        var div = document.getElementById('authortags'); 
        if (div.style.display == "none") {
            div.style.display = "block";
            document.getElementById("expandcollapseauthortags").src = "images/collapse.png";
        }
        else {
            div.style.display = "none";
            document.getElementById("expandcollapseauthortags").src = "images/expand.png";
        }
    }
</script>
