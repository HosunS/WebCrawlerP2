<script type="text/javascript">
function checkFormSubmit(client, isInstAccount, redir) {
	if ((document.forms.loginForm.username.value.length > 0)
			
		&& (document.forms.loginForm.password.value.length > 0)) {
		jQuery('#modalWindowSignInBtn').attr("disabled","disabled");
		var millisecondsToWait = 1000;
		if(IBP_WS_ENABLED_FLAG){					  
			millisecondsToWait = IBP_MEMBEER_SIGNIN_TIME_WAIT_IN_MILLIES;
			a_authenticateUserByJsonp(document.forms.loginForm.username.value,document.forms.loginForm.password.value);
			setTimeout(function() {
				
				document.forms.loginForm.submit();
			}, millisecondsToWait);				  
		}
		else
		{
			jQuery('#modalWindowSignInBtn').attr("disabled","disabled");
			document.forms.loginForm.submit();
		}
		jQuery('#modalWindowSignInBtn').removeAttr("disabled");

		return false;
		
	} else {
		alert("Please enter username and password.")
		return false;
	}
}
jQuery(document).ready(function(){
	jQuery('#modalWindowSignInBtn').on('dblclick',function(event){
		event.preventDefault();
		return false;
	});
	jQuery('.gdprAccepted').change(function(){
		
		if(this.checked){
			jQuery('#modalWindowSignInBtn').attr("title","Sign In");
			jQuery('#modalWindowSignInBtn').removeAttr("disabled");
		}else{
			jQuery('#modalWindowSignInBtn').attr("disabled","disabled");
			jQuery('#modalWindowSignInBtn').attr("title","Please Accept Consent");
		
		}
	})
});
</script>


	
	
