    <script type="text/javascript">
        // Header
        /// <reference path="http://business.fullerton.edu/WebApi/Compiler/Javascript" />

function injectGoogleSearch() {

    if (window.GS_Activated !== true)
    {
        window.GS_Activated = true;

        (function () {
            var cx = '009229506122792839394:jgmrdd33roy';
            var gcse = document.createElement('script');
            gcse.type = 'text/javascript';
            gcse.async = true;
            gcse.src = (document.location.protocol == 'https:' ? 'https:' : 'http:') +
                '//cse.google.com/cse.js?cx=' + cx;
            var s = document.getElementsByTagName('script')[0];
            s.parentNode.insertBefore(gcse, s);
        })();
    }

}

$(document.body).off("click", ".menu-control");

$(document.body)
    .on("click", ".menu-control", menuTitleClick);

function menuTitleClick(e) {

    var myId = $(this).next("div").attr("id");

    $(".menu-control").not("[data-section='" + myId + "']").removeClass("menu-control-focus");

    $(this).toggleClass("menu-control-focus");

    $(".flyout-menu").not("#" + myId).removeClass("flyout-active");

    $("#" + myId).toggleClass("flyout-active");
}

function menuMobileClick(e) {
    var myId = $(this).parent().next("div").attr("id");

    $(".menu-control").not("[data-section='" + myId + "']").removeClass("mobile-menu-control-focus");

    $(this).toggleClass("mobile-menu-control-focus");

    $(".flyout-menu").not("#" + myId).removeClass("mobile-flyout-active");

    $("#" + myId).toggleClass("mobile-flyout-active");
}

function resizeCheck() {

    debugger;
    if (app.checkScreenSize().tablet || app.checkScreenSize().phone) {
        $(document.body).off("click", ".menu-control");

        $(document.body).on("click", ".menu-control", menuMobileClick);
    } else {

        $(document.body).off("click", ".menu-control");

        $(document.body).on("click", ".menu-control", menuTitleClick);

        $("#MainBanners-Mid-Nav").show();
    }
}

app.Jobs.showMobileMenu = function (e) {

    $("#MainBanners-Mid-Nav").toggle();
    $("#MobileMenu").toggleClass("inactive");
};

window.clearMenuSelections = function () {
    $(".flyout-menu").removeClass("flyout-active");
    $(".menu-control").removeClass("menu-control-focus");
    $(".flyout-menu").removeClass("mobile-flyout-active");
    $(".menu-control").removeClass("mobile-menu-control-focus");
};

$(window).on("resize", resizeCheck);

resizeCheck();


// New Demo Search Implementation

app.Jobs.showDesktopSearch = function (e) {

    injectGoogleSearch();

    $("#DesktopSearchBarWrapper").css("margin-top", "170px");

    $("#DesktopSearchBarWrapper").animate({ opacity: "1.0" }, 450);

    $("#DesktopSearchActivator").data("click", "hideDesktopSearch");

    $("#SearchBarCloser").show();

};

app.Jobs.hideDesktopSearch = function (e) {

    $("#DesktopSearchBarWrapper").animate({ opacity: "0.0" }, 450, function () {
        $("#DesktopSearchBarWrapper").css("margin-top", "-75px");
    });

    $("#DesktopSearchActivator").data("click", "showDesktopSearch");

    $("#SearchBarCloser").hide();
};

        //Body
        /// <reference path="http://Business.Fullerton.Edu/WebApi/Compiler/JavaScript?debugmode=true" />


        // Footer
        

        //Ready Event
        $(document.body).trigger("componentReady.Default", {});
    </script>
