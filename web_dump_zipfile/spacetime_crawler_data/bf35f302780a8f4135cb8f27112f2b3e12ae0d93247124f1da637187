        function canaryTrack(event, properties, kopernioEvent) {
          if (PLUGIN_VERSION) {
            var props = properties || {};
            
            sendCanaryMessage({
              action: 'canary-track',
              trackEvent: event,
              trackProperties: props
            });
          } else if (kopernioEvent){
            // This is used by plugin-uninstalled.html
            
            var props = properties || {};
            
            props.type = event;
            props.context_id = genUUID();
            props.timestamp=  new Date().toISOString();
            props.url = window.location.href;

            var formData = null;
            formData = new FormData();
            formData.append('events', JSON.stringify([props]));

            var xhr = new XMLHttpRequest();
            xhr.open('POST', KOPERNIO_API + '/save-events');
            xhr.withCredentials = true;
            xhr.send(formData);
          } else {
            mixpanel.track(event, properties);
          }
        }

