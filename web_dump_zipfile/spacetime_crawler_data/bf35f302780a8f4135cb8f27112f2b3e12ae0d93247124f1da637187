<script>

function goToWebStore() {
  window.location = CHROME_EXTENSION_URL;
}

function showInstallPrompt(browser) {
  $('.install-prompt').show();
  $('.' + browser + '-install-prompt').show();
}


function firefoxInstallPopup() {
  if (typeof InstallTrigger !== 'undefined') {
    InstallTrigger.install({
      'Kopernio - powered by Web of Science': FIREFOX_EXT_SRC
    });
  } else {
    // Fallback method - it sometimes results in the arrow image
    // not being loaded so delay it.
    setTimeout(function() {
      window.location = FIREFOX_EXT_SRC;
    }, 1000);
  }
}

$(document).ready(function() {
    
    
    sendCanaryMessage({action: 'clearuserdata'});
    

    if (IS_FIREFOX) {
      $('.chrome-dl').hide();
      $('.firefox-dl').show();
      $('.firefox-dl').on('click', function() {
        trackInstall();
        showInstallPrompt('firefox');
        // 'Try again' link.
        $('.install-prompt-accident a').on('click', function(e) {
          firefoxInstallPopup();
          e.preventDefault();
        });
        firefoxInstallPopup();
      });
    } else if (!IS_CHROME) {
      
      $('.chrome-dl > span').text('Install Kopernio free'); // browser agnostic
      $('.chrome-dl').one('click', function(e) {
        e.preventDefault();
        $('.unsupported').slideDown();
        if (IS_MOBILE) {
          $('.chrome-dl > span').text('Kopernio plugin');
        } else {
          $('.chrome-dl > span').text('Kopernio in the Chrome Web Store');
        }
        $('.chrome-dl').on('click', function() {
          trackInstall();
          goToWebStore();
        });
      });

      if (IS_MOBILE) {
        $('.mobile-warning').show();
        typeof ga !== 'undefined' && ga('send', 'event', 'link-reminder', 'show-mobile', navigator.userAgent);
      } else if (IS_UNSUPPORTED_DESKTOP) {
        $('.desktop-warning').show();
        typeof ga !== 'undefined' && ga('send', 'event', 'link-reminder', 'show-desktop', navigator.userAgent);
      }
    }


    $('[data-toggle="tooltip"]').tooltip({
      container: 'body'
    });

    $('.reminder-form > form').on('submit', function(e) {
      e.preventDefault();
      var email = $('.reminder-form input[type=email]').val();
      if (email.length > 0) {
        $('#reminder-fail').hide();
        $.ajax({
          url: '/api/v1/remind-me-link',
          method: 'POST',
          data: {email: email},
          success: function() {
            $('.reminder-form > form').hide();
            $('#reminder-success').show();
            typeof ga !== 'undefined' && ga('send', 'event', 'link-reminder', 'sent', email);
          },
          error: function() {
            $('#reminder-fail').show();
            $('.reminder-form input[type=email]').focus();
          }
        });
      } else {
        $('.reminder-form input[type=email]').focus();
      }
    });
});
</script>

<script type="text/javascript">

$(document).ready(function (){
    function goToManualInstall(){
        $('a.plugin-dl.chrome-dl').each(function(i, e){
            $(this).attr('href', "/install-manually");
            $(this).attr('target', "_self");
        })
    }

    if (IS_CHROME){
        checkAccessToChromeStore(function(){}, goToManualInstall);
    }
});

</script>

<script type="text/javascript">
    function truncateString(str) {
      if (!str || str.length < 250) {
        return str;
      }
      return str.substring(0, 250) + '...';
    }


    $(document).ready(function(){
      canaryTrack('Web: Landing');
      canaryTrack('Web: Home');
      var idCounter = 0;
      Mustache.tags = ['[[', ']]']; //No conflict with jinja2
      $.getJSON({
          url: "/api/v1/sample-recently-added",
        xhrFields: {
          withCredentials: true
        },
        success: function(data) {
          data.pubs = data.pubs.map(function(item) {
            item.displayTitle = truncateString(item.title);
            item.displayAuthors = item.authors;
            item.id = 'pub-' + idCounter;
            idCounter += 1;
            return item;
          });

          var template = $('#pub-template').html();
          for (var i = 0; i < data.pubs.length ; i++) {
            if(i < 2){
              var html = Mustache.render(template, data.pubs[i]);
              $('.pub-list').append(html);
            }
            var img = new Image();
            img.src = data.pubs[i]['thumbnail'];
          }

          var iso = $('.pub-list').isotope({
              itemSelector: '.pub-item',
              layoutMode: 'fitRows'
          });

          var pubs = data.pubs;

          var i = 0;
          function updatePubs(){
            var el = $('#' + pubs[i % pubs.length].id);
            iso.isotope('remove', el).isotope('layout');
            el.remove();

            i += 1;
            var html = Mustache.render(template, data.pubs[(i+1) % pubs.length]);
            iso.isotope('insert', $(html)).isotope('layout');;

            setTimeout(updatePubs, 2000 + Math.random() * 5000);
          }
          if (pubs.length > 0) updatePubs();
        }
      });
    });



