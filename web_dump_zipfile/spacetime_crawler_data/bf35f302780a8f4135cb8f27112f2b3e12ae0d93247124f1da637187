        var IS_MOBILE = navigator.userAgent.search('Mobi') >= 0 ||
          navigator.userAgent.search('Android') >= 0 ||
          navigator.userAgent.search('CriOS') >= 0;
        var IS_CHROME = !IS_MOBILE &&
          navigator.userAgent.search('Chrome/[.0-9]+') >= 0 &&
          navigator.userAgent.search('Chromium/[.0-9]+') == -1 &&
          navigator.userAgent.search('OPR') == -1 &&
          navigator.userAgent.search('Edge') == -1;
        var IS_FIREFOX = !IS_MOBILE &&
          navigator.userAgent.search('Firefox/[.0-9]+') >= 0 &&
          navigator.userAgent.search('Seamonkey/[.0-9]+') == -1;
        var IS_UNSUPPORTED_DESKTOP = !IS_MOBILE && !IS_CHROME && !IS_FIREFOX;

