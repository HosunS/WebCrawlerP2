        }

        function showUpdateExtensionAlert(){
          
          const actionText = 'Please click here to update your Kopernio plugin to version ' + FIREFOX_PLUGIN_VERSION_LATEST;
          let updateUrl = $('<a>');
          updateUrl.append(actionText);
          updateUrl.css({'color':'white'});
          updateUrl.attr('href', "/install-manually?action=update");

          $('.alert-content').empty().append(updateUrl).show();
          $('.alert-bar').show();
        }

        function genUUID() {
          return ([1e7]+-1e3+-4e3+-8e3+-1e11).replace(/[018]/g, c =>
            (c ^ crypto.getRandomValues(new Uint8Array(1))[0] & 15 >> c / 4).toString(16)
          )
        }

