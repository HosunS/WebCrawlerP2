public:
  Lexic(const vector<string> *d, const unsigned clusterNo, 
        const SampleType sampleType, const unsigned samplePer = 20,
        const unsigned queueSize = 10, const unsigned uniqueNo = 10);

  void buildClusters();

  ostream& info(ostream& out);
};

