            <div class="alerts-item alerts-list__item alerts-list__item--parent" ng-click="route.Open = !route.Open">
                <div class="alerts-item__number" ng-bind="route.rt"></div>
                <div class="alerts-item__name media__content" ng-bind="route.nm"></div>
                <div class="alerts-item__detail-toggle">
                    <span class="alerts-item__count" ng-bind="routeAlerts.length"></span>
                    <span class="media__icon">
                        <svg class="alert-icon alert-icon-small box-margin-sm">
                            <use xlink:href="#general" />
                        </svg>
                    </span>
                    <div class="result-arrow result-arrow--alert" ng-class="{'open': route.Open}" style=""></div>
                </div>
            </div>
            <div ng-show="route.Open" ng-repeat="alert in routeAlerts = getRouteAlerts(route)" class="alerts-item alerts-list__item alerts-list__item--child">
                <span class="media__icon">
                    <svg class="alert-icon alert-icon-small box-margin-sm">
                        <use xlink:href="#general" />
                    </svg>
                </span>
                <span class="media__content">
                    <div class="text-strong" ng-bind="alert.sb.brf"></div>
                    <a ng-href='{{getAlertUrl(alert)}}' class="alert-detail text-strong text-uppercase">More Details</a>
                </span>
            </div>
        </div>
        <!--Route Alerts-->


        

        <div class="flex-row">
            <div class="flex-third alert-empty">
                <div ng-show="NumberOfAlerts == 0 && Loading == false">
                    <svg class="alert-icon inline-item">
                        <use xlink:href="#smile" />
                    </svg>
                    <span class="inline-item ts-h3 text-thin">No Alerts Today</span>
                </div>
            </div>
            <div class="flex-two-third alert-login">
                <div ng-if="isAuthed">
                    <div class="content-buttons">
                        <a class="btn--alt" ng-href="/Schedules-Maps-and-Tools/Signup-for-Alerts" target="_self">Manage My Alert Subscriptions</a>
                    </div>
                </div>
                <div ng-if="!isAuthed">
                    <div class="content-buttons">
                        <strong class="text-uppercase">Get Alerts by Email:
                            <!--and / or Phone-->
                        </strong>
                        <div class="u--inline-block">
                            <a class="btn btn--alt btn--wide" ng-href="/login" target="_self">Sign Up</a> or <a class="btn btn--alt btn--wide" ng-href="/login?returnurl=/Schedules-Maps-and-Tools/Signup-for-Alerts" target="_self">Login</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">
      
    var isAuthed = false;
  

    var HomeAlertsApp = angular.module("HomeAlertsApp", []);

    HomeAlertsApp.config(['$httpProvider', function ($httpProvider) {
        //disable caching for IE 11
        if (!$httpProvider.defaults.headers.get) {
            $httpProvider.defaults.headers.get = {};
        }
        $httpProvider.defaults.headers.get['Cache-Control'] = 'no-cache';
        $httpProvider.defaults.headers.get['Pragma'] = 'no-cache';
    }]);

    HomeAlertsApp.controller("HomeAlertsCtrl", function ($scope, $http, $sce, $window) {
        $scope.Loading = true;
        $scope.isAuthed = $window.isAuthed;
        $scope.Alerts = [];
        $scope.Routes = [];
        $scope.HasSystemWideAlert = false;
        $scope.NumberOfAlerts = "";

        $scope.getAlertUrl = function (alert) {
            var url = '/Schedules-Maps-and-Tools/Service-Alerts/' + encodeURIComponent(alert.sb.nm.replace(/:/g, '--colon--').replace(/\//g, '--slash--'))
            if (alert.sb.srvc != null) {
                var route = undefined;
                var stop = undefined;
                if (!Array.isArray(alert.sb.srvc)) {
                    if (alert.sb.srvc.rt)
                        route = alert.sb.srvc.rt;
                    if (alert.sb.srvc.stpid)
                        stop = alert.sb.srvc.stpid;
                }
                else {
                    for (i = 0; i < alert.sb.srvc.length; i++) {
                        if (alert.sb.srvc[0].rt && !route) {
                            route = alert.sb.srvc[0].rt;
                        }
                        if (alert.sb.srvc[0].stpid && !stop) {
                            stop = alert.sb.srvc[0].stpid;
                        }
                    }
                }
                url += '/' + route;
                if (stop)
                    url += '/' + stop;
            }
            return url
        };

        $scope.getAlertType = function (alert) {
            if (alert.sb.srvc != null) {
                if (!Array.isArray(alert.sb.srvc)) {
                    if (alert.sb.srvc.stpid)
                        return "Stop";
                    return "Route";
                }
                else {
                    for (i = 0; i < alert.sb.srvc.length; i++) {
                        if (alert.sb.srvc[0].stpid) {
                            return "Stop";
                        }
                    }
                    return "Route";
                }
            }
            return "ServiceWide"
        };

        $scope.getRouteAlerts = function (route) {
            return $scope.Alerts.filter(function (a) {
                if (!a.sb.srvc)
                    return false;
                else if (Array.isArray(a.sb.srvc)) {
                    if (a.sb.srvc.filter(function (srvc) { return route.rt == srvc.rt }).length > 0)
                        return true;
                }
                else {
                    return a.sb.srvc.rt == route.rt;
                }
            });
        };

        $scope.getSystemWideAlerts = function () {
            return $scope.Alerts.filter(function (a) { return !a.sb.srvc; });
        };

        $scope.SetRouteName = function (route) {
            $http({
                method: 'GET',
                url: '/DesktopModules/AATA.EndPoint/Proxy.ashx',
                params: { method: 'getroutebyid', routeid: route.rt }
            }).then(function success(response) {
                route.nm = response.data.route.nm;
            });
        };

        $http({
            method: 'GET',
            url: '/DesktopModules/AATA.EndPoint/Proxy.ashx',
            params: { method: 'getservicebulletins', sort: false }
        }).then(function success(response) {
            $scope.NumberOfAlerts = response.data.length;
            $scope.Alerts = response.data;
            $scope.Alerts.forEach(function (a) {
                if (a.sb.srvc) {
                    if (Array.isArray(a.sb.srvc)) {
                        a.sb.srvc.forEach(function (srvc) {
                            if ($scope.Routes.filter(function (r) { return r.rt == srvc.rt }).length == 0) {
                                var route = { rt: +srvc.rt, nm: "" };
                                $scope.Routes.push(route);
                            }
                        });
                    }
                    else {
                        if ($scope.Routes.filter(function (r) { return r.rt == a.sb.srvc.rt }).length == 0) {
                            var route = { rt: +a.sb.srvc.rt, nm: "" }
                            $scope.Routes.push(route);
                        }
                    }
                }
                else {
                    $scope.HasSystemWideAlert = true;
                }
            });
            $scope.Routes = $scope.Routes.sort(function (a, b) { return a.rt - b.rt });
            if ($scope.HasSystemWideAlert) {
                $scope.Routes = $scope.Routes.splice(0, 2);
            }
            else {
                $scope.Routes = $scope.Routes.splice(0, 3);
            }
            $scope.Routes.forEach($scope.SetRouteName);

            $scope.Loading = false;
        });
    });

    angular.bootstrap($("#HomeAlertsApp"), ["HomeAlertsApp"]);
</script>


</div><!-- End_Module_3629 --></div>
	<div class="clear"></div>
</div>

</div></div>
            </div>

