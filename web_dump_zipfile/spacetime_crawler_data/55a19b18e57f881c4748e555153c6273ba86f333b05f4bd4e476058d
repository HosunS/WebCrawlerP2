    var app = angular.module('HomeToolsApp', ['ngAnimate', '720kb.datepicker', 'ngMaterial']);

    app.config(['$httpProvider', '$mdThemingProvider', function ($httpProvider, $mdThemingProvider) {
        //disable caching for IE 11
        if (!$httpProvider.defaults.headers.get) {
            $httpProvider.defaults.headers.get = {};
        }
        $httpProvider.defaults.headers.get['Cache-Control'] = 'no-cache';
        $httpProvider.defaults.headers.get['Pragma'] = 'no-cache';
        var aaataAdd = $mdThemingProvider.extendPalette('pink', {
          '500': '#aaaaaa',
          'contrastDefaultColor': 'light',    // whether, by default, text (contrast)
                                              // on this palette should be dark or light
        });
        $mdThemingProvider.definePalette('aaataPalette', aaataAdd);
        $mdThemingProvider.theme('default')
          .primaryPalette('aaataPalette')
    }]);

    app.controller("HomeToolsCtrl", HomeToolsCtrl);
    app.controller("TrackMyBusCtrl", TrackMyBusCtrl);
    app.controller("PlanMyTripCtrl", PlanMyTripCtrl);
    app.controller("SchedulesCtrl", SchedulesCtrl);
