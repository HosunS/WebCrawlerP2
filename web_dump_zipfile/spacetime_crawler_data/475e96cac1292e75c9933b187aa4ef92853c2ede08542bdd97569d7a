<script>
function rotateImages() {
  var thumbImages = new Array();
    
    thumbImages[0] = "http://www.themesltd.com/img/theme_previews/1.png";
    thumbImages[1] = "http://www.themesltd.com/img/bythree_cover_ex.png";
    thumbImages[2] = "http://www.themesltd.com/img/trngl_cover.png";
    thumbImages[3] = "http://www.themesltd.com/img/to_the_edge_cover_ex.png";
    thumbImages[4] = "http://www.themesltd.com/img/nine_to_five_cover.png";
    thumbImages[5] = "http://www.themesltd.com/img/solid_column_cover_ex.png";
    thumbImages[6] = "http://www.themesltd.com/img/2_wide_column_cover_ex.png";
    thumbImages[7] = "http://www.themesltd.com/img/3_column_cover_ex.png";
    thumbImages[8] = "http://www.themesltd.com/img/2_column_cover_ex.png";
    thumbImages[9] = "http://www.themesltd.com/img/normal_cover_ex.png";
    thumbImages[10] = "http://www.themesltd.com/img/grid_cover_ex.png";
    thumbImages[11] = "http://www.themesltd.com/img/slick_cover_ex.png";
    thumbImages[12] = "http://www.themesltd.com/img/againstthewall_cover_ex.png";

  var images = document.getElementsByClassName("thumb__rotate");
  for (var i = 0; i < images.length; i += 1) {
    var image = images[i];
    var randomImageIndex = Math.floor(Math.random() * thumbImages.length);


    image.src = thumbImages[randomImageIndex];
  }
}
window.setInterval(rotateImages, 3250);
</script>
