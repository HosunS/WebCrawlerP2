<script>
    //====================================================
    submitPassword=function(guide_id) {
        springSpace.UI.notify({mode: "load", duration: 30000});
        xhr = jQuery.ajax({
            url: "/auth_process.php",
            data: {
                action: 752,
                obj_type: 1,
                obj_id: 638256,
                password: jQuery("#password").val()
            },
            type: "POST",
            dataType: "json",
            success: function(response, textStatus, jqXHR) {
                if ( response.errCode == 200 ) {
                    if (response.data.result == 1) {
                        springSpace.UI.notify({mode: "success", duration: springSpace.UI.CONST.conf_close_delay});
                        setTimeout(function() { window.location.reload();
                        }, springSpace.UI.CONST.conf_close_delay);
                    }
                    else {
                        springSpace.UI.notify({msg: "The provided password was incorrect - please try again!", duration: springSpace.UI.CONST.conf_close_delay});
                        jQuery("#password").val("")
                    }
                }
                else {
                    springSpace.UI.notifyStop();
                    springSpace.UI.error(response.errCode);
                }
            },
            error: function (jqXHR, textStatus, errorThrown) {
                springSpace.UI.notifyStop();
                springSpace.UI.error(errorThrown);
            }
        });

        return false;
    }
    //====================================================
</script>
