<script type="text/javascript">
    var currentTop10Idx = 1;
    var previousTop10Idx = 0;
    function top10Next() {
        var div = document.getElementById( "sm-spotlights" + currentTop10Idx );
        if( ! div ) {
            currentTop10Idx = 0;
            div = document.getElementById( "sm-spotlights" + currentTop10Idx );
        }
        
        document.getElementById( "sm-spotlights" + previousTop10Idx ).style.display = "none";
        div.style.display = "inline";
        previousTop10Idx = currentTop10Idx++;
        
    }
    function top10Prev() {
        currentTop10Idx -= 2;
        var div = document.getElementById( "sm-spotlights" + currentTop10Idx );
        if( ! div ) {
            currentTop10Idx = TOTAL_REASONS_ITEMS - 1;
            div = document.getElementById( "sm-spotlights" + currentTop10Idx );
        }
        
        document.getElementById( "sm-spotlights" + previousTop10Idx ).style.display = "none";
        div.style.display = "inline";
        previousTop10Idx = currentTop10Idx++;
        
    }
</script>
