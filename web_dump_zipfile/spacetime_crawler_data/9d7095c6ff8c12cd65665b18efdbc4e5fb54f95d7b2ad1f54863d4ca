<script type="text/javascript">
    var currentnewsIdx = 1;
    var previousnewsIdx = 0;
    function newsnext() {
        var div = document.getElementById( "sm-news" + currentnewsIdx );
        if( ! div ) {
            currentnewsIdx = 0;
            div = document.getElementById( "sm-news" + currentnewsIdx );
        }
        
        document.getElementById( "sm-news" + previousnewsIdx ).style.display = "none";
        div.style.display = "inline";
        previousnewsIdx = currentnewsIdx++;
        
    }
    function newsprev() {
        currentnewsIdx -= 2;
        var div = document.getElementById( "sm-news" + currentnewsIdx );
        if( ! div ) {
            currentnewsIdx = TOTAL_REASONS_ITEMS - 1;
            div = document.getElementById( "sm-news" + currentnewsIdx );
        }
        
        document.getElementById( "sm-news" + previousnewsIdx ).style.display = "none";
        div.style.display = "inline";
        previousnewsIdx = currentnewsIdx++;
        
    }
