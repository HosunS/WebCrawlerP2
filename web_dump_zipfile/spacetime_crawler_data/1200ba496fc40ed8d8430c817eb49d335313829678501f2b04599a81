    function focusLoginEmail() {
        var form = document.getElementById('loginForm');
        var elements = form.elements;
        for(var i=0; i<elements.length; i++) {
            if ( elements[i].id == "email" ) {
                elements[i].focus();
                return;
            }
        }
    }

    addLoadEvent(focusLoginEmail);
</script>



    





  
