/* booleanValue()
 * @return If the Boolean object has no initial value or if it is 0, '0', -0, '-0', null,
 *    "", false, 'false', undefined, or NaN, the object is set to false; otherwise, true.
 */
function booleanValue(value) {
  if ((new Boolean(value)).valueOf() && value != 'false' && value != '0' && value != '-0') {
    return true;
  } else {
    return false;
  }
}
