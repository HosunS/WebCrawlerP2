<script type="text/javascript">
// logic for determining whether or not to show vendor group & marketscope switcher
    $(function() {
        if (typeof(localStorage) !== "undefined") {
            var storageKey = "vss-vg-switcher-data";
            var vgData = localStorage.getItem(storageKey);
            if (vgData == null) {
                retrieveAndParseVGData(true);
            } else {
                shouldShowVendorGroupSwitcher(vgData);
            }
        } else {
            retrieveAndParseVGData(false);
        }

        function retrieveAndParseVGData(storageSupport) {
            $.ajax({
                url: '/hz/vendor/members/vendor-group-switcher-data/resource',
                success: function(data) {
                    if (JSON.parse(data).length > 0 && storageSupport) {
                        try {
                            localStorage.setItem(storageKey, data);
                        } catch (e) {
                            // do nothing, just continue executing method
                        }
                    }
                    shouldShowVendorGroupSwitcher(data);
                },
                dataType: 'text'
            });
        }

        function shouldShowVendorGroupSwitcher(vgData) {
            var jsonArr = JSON.parse(vgData);
            if (jsonArr.length > 1) {
                jQuery("#vendor-group-switcher_topRightNav").show();
            } else {
                jQuery("#account-region-text").show();
                jQuery("#marketscope_switcher_content").show();
            }
        }
    });
</script>




