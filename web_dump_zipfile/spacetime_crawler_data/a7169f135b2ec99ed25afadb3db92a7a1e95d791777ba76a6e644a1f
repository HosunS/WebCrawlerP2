<!-- Scroll script --begin here -->
<script>
window.onscroll = function() {scrollFunction()};

function scrollFunction() {
    if (document.body.scrollTop > 40 || document.documentElement.scrollTop > 40) {
        document.getElementById("myBtn").style.display = "block";
    } else {
        document.getElementById("myBtn").style.display = "none";
    }
}

// When the user clicks on the button, scroll to the top of the document
function topFunction() {
    document.body.scrollTop = 0;
    document.documentElement.scrollTop = 0;
}
<!-- Scroll script --end here -->

    function foo(url, eml) {
        var form = document.createElement("form");
        form.action= url;
        form.method = "post";
        var input = document.createElement("textarea");
        input.name = "email";
        input.value = eml;
        form.appendChild(input);
        
        form.style.display = 'none';
        document.body.appendChild(form);

        form.submit();



    }

    function open_selfawareness()
        {
            var email_push = sessionStorage.getItem("email");
            if (typeof email_push == 'undefined' || email_push == null || email_push == 'null' || email_push == ''){
                
                    alert("You need to be logged in to view these apps");
                    window.open("http://sensoria.ics.uci.edu:1084");
                    return
            }
            foo("http://tippersweb.ics.uci.edu/self_awareness/pages/index.php", sessionStorage.getItem("email"));
        }
    
    function open_concierge()
        {
            var email_push = sessionStorage.getItem("email");
            if (typeof email_push == 'undefined' || email_push == null || email_push == 'null' || email_push == ''){
                
                    alert("You need to be logged in to view these apps");
                    window.open("http://sensoria.ics.uci.edu:1084");
                    return
                }
            foo("http://tippersweb.ics.uci.edu/concierge/concierge.php", sessionStorage.getItem("email"));
 
        }

    function open_noodle()
    {
            var email_push = sessionStorage.getItem("email");
            if (typeof email_push == 'undefined' || email_push == null || email_push == 'null' || email_push == ''){
                
                    alert("You need to be logged in to view these apps");
                    window.open("http://sensoria.ics.uci.edu:1084");
                    return
                }
            foo("http://tippersweb.ics.uci.edu:8069/index.html", sessionStorage.getItem("email"));
 
        }

		
		

        


</script>



