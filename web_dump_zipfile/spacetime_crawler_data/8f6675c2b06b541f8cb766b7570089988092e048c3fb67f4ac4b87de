            jQuery("input[name = 'gettag']").click(function () {
                var tagval = jQuery(this).prev().val();
                var link = jQuery(this).prev().attr('link');
                if (tagval.length > 0) {
                    jQuery(this).prev().removeAttr("style");
                    jQuery(this).closest('.url-widget').find('.trackingurl').text(link + "?tag=" + tagval);
                } else {
                    jQuery(this).prev().css("border", "2px solid #B33A3A");
                }
            });


