  <script type="text/javascript">
       function onsubmitFunc(elem){
           document.getElementById(elem).click();
       }
       
       function changeInputType1(prm) {
              var oldObject = document.getElementById(prm);
              var newObject = document.createElement('input');
              if (oldObject.type == 'text'){
                   newObject.type = 'password';
                   document.getElementById('userLabel2').style.display = "none";   
                   document.getElementById('userLabel1').style.display = ""; 
              }else {
                  newObject.type ='text';
                  document.getElementById('userLabel1').style.display = "none";   
                  document.getElementById('userLabel2').style.display = ""; 
              }
              if(oldObject.size) newObject.size = oldObject.size;
              if(oldObject.getAttribute("maxlength") != null) {
                  newObject.setAttribute("maxlength",oldObject.getAttribute("maxlength"));
              }
              if(oldObject.value) newObject.value = oldObject.value;
              if(oldObject.name) newObject.name = oldObject.name;
              if(oldObject.id) newObject.id = oldObject.id;
              if(oldObject.className) newObject.className = oldObject.className;
              oldObject.parentNode.replaceChild(newObject,oldObject);
              return;

        }
       
        function displayMessage(mes){
           alert(mess);
       }
    </script>
  
 
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.5/jquery.min.js" type="text/javascript"></script>
  <script type="text/javascript">
       function openOffersDialog() {
        $('#overlay').fadeIn('fast', function() {
        $('#boxpopup').css('display','block');
            $('#boxpopup').animate({'left':'30%'},500);
            $('#overlay').css('background','#000000');
            $('#overlay').css('bottom','0');
            $('#overlay').css('left','0');
            $('#overlay').css('position','fixed');
            $('#overlay').css('right','0');
            $('#overlay').css('top','0');
            $('#overlay').css('z-index','100');
            $('#overlay').css('opacity','0.5');
  
        });
     }
     function closeOffersDialog(prospectElementID) {
                $(function($) {
                    $(document).ready(function() {
                        $('#' + prospectElementID).css('position','absolute');
                        $('#' + prospectElementID).animate({'left':'-100%'}, 0, function() {
                            $('#' + prospectElementID).css('position','fixed');
                            $('#' + prospectElementID).css('left','100%');
                            $('#overlay').fadeOut('fast');
                        });
                    });
                });
       }
    </script>
  


