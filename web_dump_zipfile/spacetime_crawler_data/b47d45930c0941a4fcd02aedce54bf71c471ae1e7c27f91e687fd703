	<script>
		jQuery(document).ready(function($){

			$.fn.shuffle = function() {

				var allElems = this.get(),
					getRandom = function(max) {
						return Math.floor(Math.random() * max);
					},
					shuffled = $.map(allElems, function(){
						var random = getRandom(allElems.length),
							randEl = $(allElems[random]).clone(true)[0];
						allElems.splice(random, 1);
						return randEl;
					});

				this.each(function(i){
					$(this).replaceWith($(shuffled[i]));
				});

				return $(shuffled);

			};

			var $featured_standards = $('.featured-standards');

			// $featured_standards.children().shuffle();

			$featured_standards.owlCarousel({
				loop:true,
				margin:40,
				dots:false,
				responsiveClass:true,
				responsive:{
					0:{
						items:1,
						nav:true
					},
					320:{
						items:2,
						nav:true
					},
					500:{
						items:3,
						nav:true
					},
					900:{
						items:4,
						nav:true
					}
				}
			});

			var $member_logos = $('.member-logos');
			$member_logos.children().shuffle();
			$member_logos.owlCarousel({
				autoplay: true,
				loop:true,
				margin:40,
				dots:false,
				responsiveClass:true,
				responsive:{
					0:{
						items:1,
						nav:true
					},
					320:{
						items:2,
						nav:true
					},
					500:{
						items:3,
						nav:true
					},
					900:{
						items:4,
						nav:true
					}
				}
			});


		});
	</script>




