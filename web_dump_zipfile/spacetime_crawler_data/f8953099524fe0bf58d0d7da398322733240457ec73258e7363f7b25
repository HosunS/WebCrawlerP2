<script id="x-custom-js">//== Link to Accordian Element and Expand

function openAccordian(linkId){
var a = document.getElementById(linkId).lastChild;
a.setAttribute("class","x-accordion-body collapse in");
a.removeAttribute("style");
}
// add onClick Element to all Elements with defined class name
// example: "acc"
function appendOnClick(className){
var cn = document.getElementsByClassName(className);
for(var i = 0; i < cn.length; i++){
var icon = cn[i]; // set the icon object
var str1 = icon.getAttribute('href');
var str2 = str1.substring(1, str1.length); // set the link for the icon object
icon.setAttribute("onclick", "openAccordian('"+str2+"')");
}
}

// Execute function on Research Page
appendOnClick("acc"); 

// POST COOKIES:  News > PageTitle

var pageURL = window.location.href;
var pageTitle = document.title.substr(1, 30) + " . . .";
var cPage = document.getElementById("currentPage");

cPage.innerHTML = pageTitle;
cPage.setAttribute('href',pageURL);
</script>
