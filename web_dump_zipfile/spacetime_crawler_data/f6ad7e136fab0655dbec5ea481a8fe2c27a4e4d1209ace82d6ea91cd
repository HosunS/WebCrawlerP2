   .image_with_wrapped_text IMG.right  { float: right; padding-left: 1em; }




   .section  { margin: 2em 0;   NOTborder: 1px dotted orange; }
   .entry    { margin: 2em 0;   NOTborder: 1px dotted purple; }

   IMG.wrapped_image         { width: 260px; padding-bottom: 1em; }
