	var selectedProviderAlias;
	function openIdIconClick(prmRequiresUserName, prmProviderName, prmProviderAlias)
	{
		selectedProviderAlias = prmProviderAlias;
		document.getElementById('divOpenIdUsername').style.display = 'none';

		if (prmProviderAlias == "[OpenId:OpenId]") {
			document.getElementById('divCustomOpenId').style.display = '';
			return false;
		}
		else {
			document.getElementById('divCustomOpenId').style.display = 'none';
		}

		if (prmRequiresUserName) {
			document.getElementById('divOpenIdUsername').style.display = '';
			var lbl = document.getElementById('cid_40_SsoLoginView_lblOpenIdUsername');
			lbl.innerHTML = lbl.innerHTML.replace('{provider}', prmProviderName);
			return false;
		}

		return triggerOpenIdWindow(prmProviderAlias);
	}

	function openIdUsernameClick() {
		var userName = document.getElementById('cid_40_SsoLoginView_tbOpenIdUsername').value;
		if ((userName != null) && (userName.length > 0))
		{
			return triggerOpenIdWindow(selectedProviderAlias, userName);
		}
		else
		{
			return false;
		}
	}

	function openIdCustomClick() {
		var customProvider = document.getElementById('cid_40_SsoLoginView_tbCustomOpenId').value;
		if ((customProvider != null) && (customProvider.length > 0) && (customProvider != 'http://'))
		{
			return triggerOpenIdWindow(selectedProviderAlias, null, customProvider);
		}
		else
		{
			return false;
		}
	}

	function triggerOpenIdWindow(prmProviderAlias, prmProviderUsername, prmCustomProvider) {
		
		//document.getElementById('divLogin').style.display = 'none';
		document.getElementById('divExtraLoginProviders').style.display = 'none';
		var ca = document.getElementById('divCreateAccount');
		if (ca) ca.style.display = 'none';
		var i = document.getElementById('divInstructions');
		if (i) i.style.display = 'none';
		document.getElementById('cid_40_SsoLoginView_divWaiting').style.display = '';

		
		var url = 'https://secure.connect.uci.edu/controls/login/OpenIdProxy.aspx?sid=1735&gid=3&oidreturnurl=https%3A%2F%2Fsecure.connect.uci.edu%2Fs%2F1735%2Finterior-1col.aspx%3Fsid%3D1735%26gid%3D3%26pgid%3D998%26cid%3D3473' + '&trigger=1&oidalias=' + encodeURIComponent(prmProviderAlias);
		if (prmProviderUsername != null) url += '&oidusername=' + encodeURIComponent(prmProviderUsername);
		if (prmCustomProvider != null) url += '&oidcustom=' + encodeURIComponent(prmCustomProvider);
		//imod.Browser.OpenWindow(url, 950, 500, "location=1,menubar=0,toolbar=0,directories=0,status=0,scrollbars=0,resizable=1", 1);
		document.location = url;

		return false;
	}
