WITH num_originals AS (SELECT COUNT(*)::float AS num FROM Tweets WHERE in_reply_to_status_id IS NULL),
     num_replies AS (SELECT COUNT(*)::float AS num FROM Tweets WHERE in_reply_to_status_id IS NOT NULL)
SELECT o.num / (o.num + r.num) AS original_fraction,
       r.num / (o.num + r.num) AS reply_fraction
FROM num_originals o, num_replies r;
 original_fraction | reply_fraction 
-------------------+----------------
            0.9944 |         0.0056
(1 row)

WITH TopTweeters AS
   (SELECT user_id, COUNT(*) AS num_tweets
    FROM Tweets
    GROUP BY user_id
    ORDER BY num_tweets DESC
    LIMIT 20)
SELECT u.id, u.name, u.screen_name, t.num_tweets,
       CURRENT_TIMESTAMP - u.created_at AS how_long,
       s.statuses_count, s.followers_count
FROM TopTweeters t, Users u, UserStats s
WHERE t.user_id = u.id AND t.user_id = s.user_id
ORDER BY t.num_tweets DESC;
