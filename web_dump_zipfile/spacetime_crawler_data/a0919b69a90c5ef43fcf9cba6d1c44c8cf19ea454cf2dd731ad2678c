print("model: y ~ 1 + t + t^2 + t^3 + t^4");
print("design variable: 0<t<3 discretized");

n=20;
## m=5;
t=3*(1:n)/n;
X=cbind(1, t, t^2, t^3, t^4);    

source('cocktail.r');
w=cocktail(X)[[1]];

print("output:");
print("optimal support points t(i)");
print(t[w>0]);
print("optimal weights w(i)");
print(w[w>0]);
