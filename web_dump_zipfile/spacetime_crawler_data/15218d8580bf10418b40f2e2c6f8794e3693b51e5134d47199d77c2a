            function onSignIn(googleUser) {
        // Useful data for your client-side scripts:
                var profile = googleUser.getBasicProfile();
                console.log("ID: " + profile.getId()); // Don't send this directly to your server!
                

                email = profile.getEmail();
                sessionStorage.setItem("email", email);

                fname = profile.getGivenName();
                sessionStorage.setItem("fname", fname);        

                lname = profile.getFamilyName();
                sessionStorage.setItem("lname", lname);       
                
                
                console.log('Full Name: ' + profile.getName());
                console.log('Given Name: ' + profile.getGivenName());
                console.log('Family Name: ' + profile.getFamilyName());
                console.log("Image URL: " + profile.getImageUrl());
                console.log("Email: " + profile.getEmail());
           
                var yourUrl = "http://sensoria.ics.uci.edu:8059/user/registered?user_id=" + sessionStorage.getItem("email");
                var todo = $.ajax ({
                          url : yourUrl,
                          headers: {
                                 'Accept': 'application/json',
                          'Content-Type': 'application/json'
                                 },
                          type: 'GET',
                      //data : JSON.stringify(temp),
                      success: function(resultData){
                            console.log(resultData);
                            if (resultData.status == 'true')
                            {
                                console.log("true");
                                window.open("../index.html", "_self");
                            }
                            else if (resultData.status == 'false')
                            {
                                console.log("false");
                                window.open("../RegisterUser.html", "_self");
                            }
                          
                            else
                            {
                                console.log("Oops");
                                alert("Please email ucitippers@gmail.com there seems to be some problem");
                            }
                          
                          
                          }

                });
              }

        </script>		


