							<label :for="tag.label" :class="{'child': !tag.parent == 0}">
								<input type="checkbox" class="hidden" :id="tag.slug" v-model="tag.checked" @change.stop="applyFilters(tag, column)">
								<span class="mc-checkbox"></span>
