model{ 
 for( i in 1:27){
   for(j in 1:4){
    Distance1[ i , j ] ~ dnorm(m[ i , j ], tau)
    m[ i , j  ]  <- (beta[1] + b[ i , 1 ]) + (beta[2] + b[ i , 2 ])*Age1[ j ]  + beta[3]*Male1[ i ]  
    } 
 }
 for( i in 1:27){
    b[ i , 1:2 ] ~ dmnorm(mu0[1:2], prec[1:2,1:2])
 }
