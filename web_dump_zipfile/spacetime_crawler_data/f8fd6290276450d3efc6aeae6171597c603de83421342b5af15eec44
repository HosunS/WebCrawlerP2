	<script> 
		//define video and play/pause button
		var vid = document.getElementById("videoBanner"); 
		var myButton = document.getElementById("btnPlayPause"); 	
		var myText = document.getElementById("txtPlayPause"); 	
	
		//detect if video is playing
		function videoStatus() {								
			if (vid.paused) {									
				vid.play();								
				myButton.src="_resources/images/icons/video-pause-icon.png";
				txtPlayPause.innerHTML = "Pause";		
			} else {									
				vid.pause();
				myButton.src="_resources/images/icons/video-play-icon.png";
				txtPlayPause.innerHTML = "Play";
			}													
		}

	</script> 
  
