<script>
    function parseXML(URL) {
        //alert(URL);
        AjaxCall(URL, "CalendarFeed");
    }

    function AjaxCall(url, results) {

        var xmlhttp;
        var method = "GET";
        xmlhttp = new XMLHttpRequest();

        xmlhttp.onreadystatechange = function () {
            if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {

                document.getElementById(results).innerHTML = xmlhttp.responseText;

            } else {
                document.getElementById(results).innerHTML = "<span class=\"loading_data\">loading data...please wait</span>";
                document.getElementById(results).innerHTML += "<br/>" + xmlhttp.statusText;
                document.getElementById(results).innerHTML += "<br/>--------------";
            }
        }

        if (method == "GET") {
            xmlhttp.open("GET", url, true);
            xmlhttp.send();
        } else {

            xmlhttp.open("POST", url, true);
            xmlhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
            xmlhttp.send(parameters);
        }
    }     

</script>

<section id="calendar">

    <h1>Calendar</h1>
    <p class="eventCategory">
        <a href="javascript:void(0)" id="btnAll" onclick="calendarSelection('all');" class="calendarButton" title="Calendar All">All</a>
        <a href="javascript:void(0)" id="btnAcademic" onclick="calendarSelection('academic');" class="calendarButton" title="Calendar Academic">Academics</a>
        <a href="javascript:void(0)" id="btnArt" onclick="calendarSelection('arts');" class="calendarButton" title="Calendar Art">Arts</a>
        <a id="btnAthletics" href="http://www.fullertontitans.com/composite" class="calendarButton" title="Calendar Athletics">Athletics</a>
        <a class="calendarButton" href="http://calendar.fullerton.edu/" title="Calendar more">more...</a>
    </p>
    <div class="calendarWrap">

        <article class="calendarPosts">
            <ul id="CalendarFeed"></ul>
        </article>
    </div>

</section>

<script>
    function calendarSelection(selection) {
        switch (selection) {
            case "all":

                $("#btnAll").addClass("calActive");
                $("#btnAcademic").removeClass("calActive");
                $("#btnArt").removeClass("calActive");

                setTimeout(function () { parseXML('CalendarPull.php?feed=all'); }, 200);
                break;
            case "academic":
                $("#btnAll").removeClass("calActive");
                $("#btnAcademic").addClass("calActive");
                $("#btnArt").removeClass("calActive");
                setTimeout(function () { parseXML('CalendarPull.php?feed=academic'); }, 200);
                break;
            case "arts":
                $("#btnAll").removeClass("calActive");
                $("#btnAcademic").removeClass("calActive");
                $("#btnArt").addClass("calActive");
                setTimeout(function () { parseXML('CalendarPull.php?feed=arts'); }, 200);
                break;
            default:
                $("#btnAll").addClass("calActive");
                $("#btnAcademic").removeClass("calActive");
                $("#btnArt").removeClass("calActive");
                setTimeout(function () { parseXML('CalendarPull.php?feed=academic'); }, 200);
        }
    }
    
    setTimeout(function () { parseXML('CalendarPull.php?feed=all'); }, 200);

    $("#btnAll").addClass("calActive");


</script>


