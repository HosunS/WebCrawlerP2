            function showSubMenu( ele ) {
                if( lastSliderSubMenu != null ) {
                    if( ele != lastSliderSubMenu ) {
                        $( lastSliderSubMenu ).siblings( 'ul' ).slideToggle();
                        $( lastSliderSubMenu ).html( "+" );
