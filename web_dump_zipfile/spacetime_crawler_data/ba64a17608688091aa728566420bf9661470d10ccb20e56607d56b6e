<script>
    $(function () {
        var titTop = $(".module-title").offset().top,
            titH = $(".module-title").innerHeight();

        $(window).scroll(function () {
            if ($(window).scrollTop() > (titTop)) {
                $(".module-title").css({position: 'fixed', top: '0', left: '0'});
            } else {
                $(".module-title").css({position: 'static'});
            }
        })

        //ÈîöÁÇπÈìæÊé•
        $(".title").click(function () {
            $(".title").removeClass('active');
            $(this).addClass('active');
            var _target = $(this).attr('id').substr(4);
            if ($(".module-title").css("position") == "fixed") {
                var rollTop = $('#' + _target).offset().top - titH;
                $("html,body").animate( {scrollTop: rollTop}, 300);
            } else {
                var rollTop = $('#' + _target).offset().top - 2 * titH;
                $("html,body").animate({scrollTop: rollTop}, 300);
            }
        })

    });
    //Ëé∑Âèñ‰ºöËÆÆlogo
    function getImageByMapId(mapId) {
        if (mapId) {
            return window.configs["sbaseUrl"] + "?mappingId=" + mapId;
        } else {
            return '../images/default.jpg';
        }
    }

</script>
