#include "scan.h"
#include "util/ed.h"

void Scan::search(const string &query, const unsigned editdist, 
                  vector<unsigned> &results)
{
  for (unsigned i = 0; i < data->size(); i++)
    if (ed(query, (*data)[i], editdist))
      results.push_back(i);
}

