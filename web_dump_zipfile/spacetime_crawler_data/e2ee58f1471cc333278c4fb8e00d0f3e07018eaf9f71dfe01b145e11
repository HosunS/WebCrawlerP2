	<script type="text/javascript" defer="defer">
		// Animate the Startup Wizard promo images

		var tcWizardImages = [];
		var tcWizardImgNo = 0;
		var tcWizardDelay = 2500;		//msec
		var tcWizardFadeDelay = 300;	//msec
		var tcWizardTimer = null;

		function tcLoadWizardImages() {
			var imgs = [];
			for (var i = 1; ; i++) {
				var img = document.getElementById("wizard-img-" + i);
				if (!img)
					break;
				imgs[i-1] = img;
			}
			tcWizardImages = imgs;
			tcWizardImgNo = imgs.length - 1;
		}

		function tcRotateWizardImages() {
			// Hide current image
			var img1 = tcWizardImages[tcWizardImgNo];
			$(img1).animate(
				{ opacity: 0.0 },
				tcWizardFadeDelay,
				"linear",
				function() { img1.style.display = "none"; }
			);

			// Show next image
			tcWizardImgNo = ++tcWizardImgNo % tcWizardImages.length;
			var img2 = tcWizardImages[tcWizardImgNo];
			img2.style.opacity = 0.0;
			img2.style.display = "block";
			$(img2).animate(
				{ opacity: 1.0 },
				tcWizardFadeDelay,
				"linear"
			);

			// Pause and show next image
			if (tcWizardTimer)
				clearTimeout(tcWizardTimer);
			tcWizardTimer = setTimeout(
				function(n) {
					tcRotateWizardImages();
				},
				tcWizardDelay);
		}

		// Load and animate/rotate the promo images
		tcLoadWizardImages();
		tcRotateWizardImages();
	</script>

